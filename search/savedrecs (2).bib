DOI = {10.32615/ps.2019.061},
Keywords = {early-successional species; gas exchange; late-successional species;
   photosynthetic photon flux density},
Keywords-Plus = {PHOTORESPIRATION; FOREST; ECOPHYSIOLOGY; IRRADIANCE; DIVERSITY; EXCHANGE},
