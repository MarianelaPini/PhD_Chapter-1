   Goncalves, Fabio Guimaraes, Woods Hole Res Ctr, Falmouth, MA 02540 USA.
   Keller, Michael Maier, Univ New Hampshire, USDA Forest Serv, Durham, NH 03824 USA.},
DOI = {10.1117/12.2066031},
