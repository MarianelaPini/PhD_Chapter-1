Keywords = {Xingu Indigenous Territory; Kawaiwete people; Ikpeng people; local
   ecological knowledge; shifting cultivation; swidden fallows; assisted
   natural regeneration; indigenous forest management},
Keywords-Plus = {SLASH-AND-BURN; SHIFTING CULTIVATION; LANDSCAPE RESTORATION; NATURAL
   REGENERATION; LAND-USE; AGRICULTURE FRONTIERS; SWIDDEN CULTIVATION;
   ECOLOGICAL ASPECTS; SOIL FERTILITY; MATO-GROSSO},
Author-Email = {msmarcustope2@gmail.com},
