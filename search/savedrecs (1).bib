@article{ WOS:000345094700004,
Author = {do Nascimento, Ladivania Medeiros and de Sa Barretto Sampaio, Everardo
   Valadares and Nogueira Rodal, Maria Jesus and Borges Lins-e-Silva, Ana
   Carolina},
Title = {Secondary succession in a fragmented Atlantic Forest landscape: evidence
   of structural and diversity convergence along a chronosequence},
Journal = {JOURNAL OF FOREST RESEARCH},
Year = {2014},
Volume = {19},
Number = {6},
Pages = {501-513},
Month = {DEC},
Abstract = {Changes in physiognomy, species composition and structure, and dispersal
   mechanisms of canopy and subcanopy plant assemblages were investigated
   along a chronosequence of three ages: 12, 20, and 50+ years old
   (=old-growth), three replications in each, in an Atlantic Forest
   landscape in Northeastern Brazil. Our objective was to investigate
   whether there is floristic and structural convergence along secondary
   succession. There were significant differences between secondary and
   old-growth forests in density and basal area only for the subcanopy.
   Differences in density between forest ages were noted when the
   assemblage was analyzed per diameter and height classes. Richness of
   canopy species of both secondary ages differed from those of old-growth
   forests. Some dominant species in the canopy of secondary forests showed
   a significant decrease in density with increasing age, which indicates
   an ongoing process of floristic changes. The low level of shared species
   between secondary and old-growth forests supports the idea that species
   composition is one of the last components to recover during successional
   process. Zoochory was the most important dispersal guild in species
   percentage and number, irrespective of stand age. Although regenerating
   areas can take alternative pathways, our results indicate that secondary
   Atlantic Forest sites have a high potential for natural regeneration.
   This recovery is recorded as a physiognomic convergence of the canopy
   layer in as little as 12 years, and progressive introductions of later
   successional species into the plant assemblage that lead to convergence
   in terms of the diversity and richness of the subcanopy and of dispersal
   guilds just 20 years after abandonment.},
Type = {Article},
Affiliation = {do Nascimento, LM (Corresponding Author), Jardim Botan Recife, Rodovia BR 232,Km 7,5 S-N, Recife, PE, Brazil.
   do Nascimento, Ladivania Medeiros, Jardim Botan Recife, Recife, PE, Brazil.
   de Sa Barretto Sampaio, Everardo Valadares, Univ Fed Pernambuco, Dept Energia Nucl, Ctr Tecnol, BR-50740540 Recife, PE, Brazil.
   Nogueira Rodal, Maria Jesus; Borges Lins-e-Silva, Ana Carolina, Univ Fed Rural Pernambuco, Dept Biol, BR-52171900 Recife, PE, Brazil.},
DOI = {10.1007/s10310-014-0441-6},
Keywords = {Composition; Dispersal mechanisms; Secondary succession; Structure},
Keywords-Plus = {OLD-GROWTH FOREST; SEED DISPERSAL; RAIN-FORESTS; SPECIES COMPOSITION;
   COMMUNITY DYNAMICS; ABANDONED PASTURES; TREE COMMUNITIES; REGENERATION;
   RECOVERY; LONG},
Author-Email = {ladivania@hotmail.com},
Times-Cited = {15},
