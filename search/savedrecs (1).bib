@article{ WOS:001295191300002,
Author = {Rodrigues, Julia Isabella de Matos and Martins, Walmer Bruno Rocha and
   da Silva, Lucas Lopes and Castro, Joathan Cipriano and Oliveira,
   Francisco de Assis},
Title = {Shifting cultivation in Amazonia: importance, impacts, and future
   perspectives},
Journal = {NATIVA},
Year = {2024},
Volume = {12},
Number = {3},
Pages = {419-427},
Abstract = {This study aimed to highlight the role, implications, and future outlook
   of shifting cultivation in Amazonia - an ancient practice widely
   utilized in the region. Shifting cultivation involves the turnover of
   cultivated areas, interleaving agricultural species with natural
   regeneration, and is fundamental to the survival of rural Amazonian
   populations. However, the slash-and-burn process leads to a reduction in
   biodiversity, soil fertility, and aggregation. Additionally, it poses
   negative impacts on the flora and fauna, as the allotted time for fallow
   in the diverse Amazonian contexts may not be sufficient due to the
   necessity for food production. This results in the expansion of fields
   and, consequently, an increase in degraded area. In this context, the
   use of ecological recovery techniques, both active and passive, becomes
   essential for maintaining ecosystem services. The secondary forest
   formed after shifting cultivation exhibits high biodiversity,
   contributing to carbon sequestration and storage. The future
   sustainability of shifting cultivation in Amazonia depends on achieving
   a balance between forest maintenance and food production. This requires
   active participation from the agricultural community and the
   implementation of low environmental impact technologies or techniques.},
Type = {Article},
Affiliation = {Rodrigues, JID (Corresponding Author), Univ Fed Rural Amazonia, Belem, PA, Brazil.
   Rodrigues, Julia Isabella de Matos; da Silva, Lucas Lopes; Castro, Joathan Cipriano; Oliveira, Francisco de Assis, Univ Fed Rural Amazonia, Belem, PA, Brazil.
   Martins, Walmer Bruno Rocha, Univ Fed Rural Amazonia, Capitao Poco, Para, Brazil.},
DOI = {10.31413/nativa.v12i3.17428},
Keywords = {secondary forest; ecological monitoring; fire; sustainability},
Keywords-Plus = {SLASH-AND-BURN; DRY-SEASON IRRIGATION; FOREST RECOVERY; EASTERN AMAZON;
