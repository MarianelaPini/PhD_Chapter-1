DOI = {10.1590/S0001-37652008000300017},
Keywords = {remote sensing; sedimentology; paleochannel; Marajo Island; Quaternary},
Keywords-Plus = {ESTUARINE DEPOSITS; VEGETATION CHANGES; CENTRAL AMAZONIA;
