}

@article{ WOS:000740861200001,
Author = {Alves, Alline Mendes and Espirito-Santo, Mario Marcos do and Silva,
   Jhonathan O. and Faccion, Gabriela and Sanchez-Azofeifa, Arturo and
   Ferreira, Kleiperry Freitas},
Title = {Successional and Intraspecific Variations in Leaf Traits, Spectral
   Reflectance Indices and Herbivory in a Brazilian Tropical Dry Forest},
Journal = {FRONTIERS IN FORESTS AND GLOBAL CHANGE},
Year = {2021},
Volume = {4},
Month = {DEC 24},
Abstract = {Leaf traits are good indicators of ecosystem functioning and can affect
   herbivory and leaf reflectance patterns, allowing a better understanding
   of changes in environmental conditions, such those observed during
   forest natural regeneration. The aim of this study was to evaluate the
   intraspecific variation in leaf traits and their influence on the
   pattern of herbivory and leaf reflectance in three species distributed
   along a successional gradient (early, intermediate and late stages) in a
   tropical dry forest (TDF) in northern Minas Gerais, Brazil. We sampled
   individuals of the following abundant tree species that occurred in
   multiple successional stages: Cenostigma pluviosum, Handroanthus
   ochraceus, and Tabebuia reticulata. We collected 10 leaves from each
   tree to determine the contents of chlorophyll a, b, and total,
   carotenoids and water, as well as the percentage of leaf area removed by
   herbivores and leaf specific mass (LSM). We also measured five spectral
   reflectance indices (Normalized Difference Vegetation Index-NDVI, Simple
   Ratio-SR, modified Normalized Difference-nND, modified SR-mSR and Water
   Index-WI) using a portable spectrometer. Our results showed
   intraspecific differences in most leaf traits along the successional
   gradient, suggesting that local adaptation may play an important role in
   plant community assembly. However, herbivory only differed for H.
   ochraceus in early and intermediate stages, but it was not affected by
   the leaf traits considered here. Spectral reflectance indices also
   differed among successional stage for all species together and for each
   species separately, except for T. reticulata in intermediate and late
   stages. Thus, leaf spectral signatures may be an important tool to the
   remote detection of different successional stages in TDFs, with
   implications for forest management.},
Type = {Article},
Affiliation = {Espirito-Santo, MMD (Corresponding Author), Univ Estadual Montes Claros Unimontes, Dept Biol Geral, Lab Ecol Evolutiva, Montes Claros, MG, Brazil.
   Alves, Alline Mendes, Univ Oeste Paulista, Fac Ciencias Agr, Presidente Prudente, SP, Brazil.
   Alves, Alline Mendes; Espirito-Santo, Mario Marcos do; Faccion, Gabriela; Ferreira, Kleiperry Freitas, Univ Estadual Montes Claros Unimontes, Dept Biol Geral, Lab Ecol Evolutiva, Montes Claros, MG, Brazil.
   Silva, Jhonathan O., Univ Fed Vale Sao Francisco UNIVASF, Col Ecol, Senhor Do Bonfim, Brazil.
   Sanchez-Azofeifa, Arturo, Univ Alberta, Earth \& Atmospher Sci Dept, Edmonton, AB, Canada.},
DOI = {10.3389/ffgc.2021.780299},
Article-Number = {780299},
Keywords = {leaf reflectance; leaf pigments; ecological succession; herbivory;
   tropical dry forests; intraspecific variation},
Keywords-Plus = {PLANT FUNCTIONAL TRAITS; PHENOTYPIC PLASTICITY; BOTTOM-UP;
   OPTICAL-PROPERTIES; INSECT HERBIVORES; BACKGROUND LOSSES; CHLOROPHYLL;
   TREE; GRADIENT; DIVERSITY},
