   Meyer, Sebastian T.; Hermann, Julia-Maria; Leidinger, Jan; Koch, Christiane; Kollmann, Johannes; Weisser, Wolfgang W., Tech Univ Munich, Dept Life Sci Syst, Freising Weihenstephan, Germany.},
DOI = {10.1016/j.biocon.2021.109387},
