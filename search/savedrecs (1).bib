@article{ WOS:000505069000016,
Author = {Carneiro, Renan do Vale and Navegantes-Alves, Livia de Freitas},
Title = {THE DIVERSITY OF FOREST RECOVERY EXPERIENCES PRACTICED BY FAMILY FARMERS
   IN NORTHEAST OF PARA},
Journal = {GEOAMBIENTE ON-LINE},
Year = {2019},
Number = {35},
Pages = {293-314},
Month = {SEP-DEC},
Abstract = {Family farmers in the Northeast of Para have, traditionally, developed
   forest recovery practices in their lots, and more recently have been
   adapting them. This research aimed to characterize the types of forest
   recovery, analyzing the existence of relationships between the types
   practiced and the environmental perceptions of these individuals.
   Therefore, the research was based on principles of interdisciplinarity
   and the systemic approach, and thus carried out a survey of sixty
   experiments in four Municipalities of the Northeastern Mesoregion of
   Para, Brazil. From the realization of a typology of the forest recovery
   the peculiarities of these experiences were understood, highlighting the
   main perceptions and motivations of the farmers. Thus, five main types
   of forest recovery practiced in the Region were found: Natural
   regeneration, Agroforestry backyard and the three types of agroforestry
   systems (Lowly diversified, Diversified and Highly diversified). It was
   concluded, therefore, that there is a framework of forest recovery in
   Northeastern Para, inspired by new socioenvironmental perceptions and
   motivations of different orders. This movement of forest recovery seems
   to point a new productive path to the Eastern Amazon.},
Type = {Article},
Affiliation = {Carneiro, RD (Corresponding Author), Univ Fed Para, Agr Familiar \& Desenvolvimento Sustentavel, Belem, Para, Brazil.
   Carneiro, Renan do Vale, Univ Fed Para, Agr Familiar \& Desenvolvimento Sustentavel, Belem, Para, Brazil.
   Navegantes-Alves, Livia de Freitas, Univ Fed Para, Inst Amazon Agr Familiares, Belem, Para, Brazil.},
Keywords = {Forest recovery; Family Farm; Northeastern Para},
Author-Email = {renan.vcarneiro@gmail.com
   lnavegantes@ufpa.br},
Times-Cited = {3},
Unique-ID = {WOS:000505069000016},
DA = {2025-04-03},
}

@article{ WOS:000329052000009,
Author = {Calegari, Leandro and Martins, Sebastiao Venancio and Campos, Lilian
   Cristina and Silva, Elias and Gleriani, Jose Marinaldo},
Title = {EVALUATION OF SOIL SEEDS BANK FOR FOREST RESTORATION IN CARANDAI, MG},
Journal = {REVISTA ARVORE},
Year = {2013},
Volume = {37},
Number = {5},
Pages = {871-880},
Month = {SEP-OCT},
Abstract = {The knowledge of soil seeds bank provides essential information on the
   potential of regeneration in a particular areas, allowing inferences on
   its restoration. By using 20 plots with 25 x 25 cm and 5 cm deep,
   samples of soils from different environmental situations, identified in
   a rural property in Caranda, MG, were collected (clean pasture, dirty
   pasture, coppice, eucalyptus and forest). The collected material was put
   to be germinated in a greenhouse and analyzed for eight months, where
   composition and the density of the species (herbaceous, sub-shrubby,
   shrubby and arboreal) were evaluated. The environmental situations
   presented different expressions of natural regeneration. The dirty
   pasture was the situation that presented the largest number of
   regenerating herbaceous trees, what makes it more problematic for forest
   restoration. All the environmental situations presented bank of seeds of
   arboreal and shrubby seedlings, which may help the restoration process.
   However, the success of this activity will be directly linked to the
   activities of competitor control, mainly of Urochloa decumbens (Stapf)
   R. D. Webster, an aggressive grass that germinates in every
   environmental situation. Due to the absence of species in final
   succession stage in the seed bank in the soil, there might have been
   other complementary interventions such as planting of seedlings, direct
   sowing of tree species and/or implantation of artificial galleries to
   accelerate the vegetable succession.},
Type = {Article},
Affiliation = {Calegari, L (Corresponding Author), Univ Fed Campina Grande, CSTR, Campina Grande, PB, Brazil.
   Calegari, Leandro, Univ Fed Campina Grande, CSTR, Campina Grande, PB, Brazil.
   Martins, Sebastiao Venancio; Silva, Elias; Gleriani, Jose Marinaldo, Univ Fed Vicosa, Ctr Ciencias Agr, Dept Engn Florestal, Vicosa, MG, Brazil.
   Campos, Lilian Cristina, Cimento Tupi SA, Dept Meio Ambiente, Carandai, MG, Brazil.},
DOI = {10.1590/S0100-67622013000500009},
