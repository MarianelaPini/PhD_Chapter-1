@article{ WOS:000466531400015,
Author = {da Silva, Leovandes Soares and Costa, Thais Ribeiro and Vieira, Arthur
   Duarte and Porfirio Franco, Stenio Abdanur and Mendonca Machado, Evandro
   Luiz},
Title = {Variations in the floristic composition in seasonal semidecidal forests
   in Curvelo-MG},
Journal = {NATIVA},
Year = {2019},
Volume = {7},
Number = {1},
Pages = {109-116},
Month = {JAN-FEB},
Abstract = {The objective of this study was to evaluate the changes in floristic
   composition in a fragment of Seasonal Semideciduous Forest. In the first
   inventory carried out in 2011, 25 plots of 10 x 40m were allocated. All
   living individuals with a breast height diameter >= 5 cm were measured.
   The second inventory was carried out in 2015, using the same criteria of
   the previous inventory, thus, were incorporated the new individuals
   (recruits) that reached the minimum diameter of inclusion. The floristic
   composition changes, the mortality of the individuals was superior to
   the recruitment, there were loss of five species and increase of two
   species. The number of species and the density of individuals varied
   over time. The species that disappeared were represented by few
   individuals. The loss of species reflected in the diversity index of
   Shannon (H `) and Pielou equability, (H' went from 4.05 to 3.99 and
   Pielou J `equability from 0.83 to 0.82), in the first and second
   inventory respectively, even so, the values point to a high floristic
   diversity. The changes in floristic composition were not significant,
   the loss of species was related to the less representative in number of
   individuals, among the inventories the forest remained stable.},
