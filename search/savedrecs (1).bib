@article{ WOS:000657953200001,
Author = {Mendes, Lara M. and Cesar, Ricardo G. and Uezu, Alexandre and Beltrame,
   Tiago P. and Rodriguez, Luiz C. E. and Gomes, Haroldo B. and Cullen,
   Laury},
Title = {Large canopy and animal-dispersed species facilitate natural
   regeneration in tropical forest restoration},
Journal = {RESTORATION ECOLOGY},
Year = {2021},
Volume = {29},
Number = {4},
Month = {MAY},
Abstract = {Realizing the benefits of forest restoration requires that these
   ecosystems be maintained after the senescence of planted trees through
   facilitation of natural regeneration. We analyzed the effect of tree
   canopy cover, dispersal syndrome, deciduousness, and taxon in
   facilitating natural regeneration in tropical forest restoration
   planting. Canopy cover had additive positive effect on natural
   regeneration when combined with animal dispersal or evergreen trees.
   Animal dispersal had a positive effect on facilitating natural
   regeneration abundance, while evergreen species had a positive effect on
   natural regeneration richness only. Although variation within and among
   species was high, restoration practitioners could consider using species
   with these traits to facilitate natural regeneration and hence
   development of restoration plantings.},
Type = {Article},
Affiliation = {César, RG (Corresponding Author), Univ Sao Paulo, ESALQ, Dept Forestry Sci, Av Padua Dias 11, BR-13418900 Piracicaba, SP, Brazil.
   Mendes, Lara M.; Uezu, Alexandre; Beltrame, Tiago P.; Gomes, Haroldo B.; Cullen, Laury, Ecol Res Inst IPE, Km 47, BR-12960000 Nazare Paulista, SP, Brazil.
   Cesar, Ricardo G.; Rodriguez, Luiz C. E., Univ Sao Paulo, ESALQ, Dept Forestry Sci, Av Padua Dias 11, BR-13418900 Piracicaba, SP, Brazil.},
DOI = {10.1111/rec.13406},
Article-Number = {e13406},
Keywords = {Atlantic Forest; deciduousness; ecological restoration; recruitment;
   species selection},
Keywords-Plus = {TREES},
