@article{ WOS:000326333400006,
Author = {van Haren, Joost and de Oliveira, Jr., Raimundo Cosme and Beldini,
   Patrick Troy and de Camargo, Plinio Barbosa and Keller, Michael and
   Saleska, Scott},
Title = {Tree Species Effects on Soil Properties and Greenhouse Gas Fluxes in
   East-central Amazonia: Comparison between Monoculture and Diverse Forest},
Journal = {BIOTROPICA},
Year = {2013},
Volume = {45},
Number = {6, SI},
Pages = {709-718},
Month = {NOV},
Abstract = {Tropical plantations are considered a viable option to sequester carbon
   on abandoned agricultural lands, but implications of tree species
   selection for overall greenhouse gas budgets on plantations have been
   little studied. During three wet seasons, we investigated the influence
   of nine tree species on soil pH, temperature (ST), bulk density (BD),
   moisture content water filled pore space (WFPS), and greenhouse gas
   fluxes in diverse forest sites and monoculture plantation plots. All
   sites were on clay-rich soils of the Barreiras formation, in
   east-central Amazonia, Brazil. We found that ST and BD were 0.6 degrees
   C and 0.2g/cm(3) higher in the plantation relative to the forest, and
   soil CH4, CO,(2) and N2O fluxes were, respectively, 38, 12, 62, percent
   lower in the plantation. Tree growth rates were highly variable on the
   plantation, with the mean comparable to the forest sites. Tree species
   identity mattered (P<0.01) for all soil properties and gas fluxes on the
   plantation, but only for pH, BD, WFPS, and N2O fluxes in the forest. The
   species rank order of pH and N2O fluxes in the forest, however, were
   unlike the plantation. Tree growth rates were a strong predictor for
   soil WFPS, and together with location, they also explained 75 percent of
   the mean N2O flux variation. Our study indicates that: (1) tree species
   influence soil processes; and (2) high tree growth and low soil gas
   emissions imply a reduced climate forcing effect from plantations,
   especially when planted with fast-growing legume species on abandoned
   farmland.},
Type = {Article},
Affiliation = {van Haren, J (Corresponding Author), Univ Arizona, Tucson, AZ 85738 USA.
   van Haren, Joost, Univ Arizona, Tucson, AZ 85738 USA.
   de Oliveira, Raimundo Cosme, Jr., EMBRAPA Amazonia Oriental, Santarem, Para, Brazil.
   Beldini, Patrick Troy, Univ Fed Oeste Para, Santarem, Para, Brazil.
   de Camargo, Plinio Barbosa, CENA, Sao Paulo, Brazil.
   Keller, Michael, Univ New Hampshire, Durham, NH 03824 USA.
   Saleska, Scott, Univ Arizona, Dept Ecol \& Evolut Biol, Tucson, AZ 85721 USA.},
DOI = {10.1111/btp.12061},
Keywords = {carbon dioxide; methane; nitrous oxide; plantation; tree growth;
   tropical},
Keywords-Plus = {NITROUS-OXIDE; CARBON SEQUESTRATION; TROPICAL PLANTATION; RAIN-FOREST;
   EMISSIONS; N2O; DYNAMICS; PASTURE; IMPACTS; GROWTH},
Author-Email = {jvanhare@email.arizona.edu},
Times-Cited = {8},
