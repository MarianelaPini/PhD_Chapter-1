@article{ WOS:000746964500001,
Author = {Martins, Jorge Carvalho and da Silva, Richardson Barbosa Gomes and
   Munis, Rafaele Almeida and Simoes, Danilo},
Title = {Investments in Pinus elliottii Engelm. Plantations: Real Options
   Analysis in Discrete Time},
Journal = {FORESTS},
Year = {2022},
Volume = {13},
Number = {1},
Month = {JAN},
Abstract = {Background: The commonly used methods for the financial evaluation of
   plantation forest investment projects do not incorporate uncertainties
   and ignore the value related to flexibility. The real options analysis
   makes it possible to capture these values in investment projects,
   increasing their value and return. Despite this, studies involving real
   options in forest investment projects are scarce, specifically those
   related to Pinus spp. Therefore, this study aimed to: (a) analyze
   whether the real options analysis adds value to investment projects of
   Pinus elliottii Engelm. plantations; and (b) make the real options
   analysis more accessible to forest managers and potentially increase its
   use in the investment projects of Pinus spp. plantations. Methods: We
   evaluated two investment projects in P. elliottii plantations in
   southern Brazil, which differed in the way of obtaining the land for
   planting: with lease or purchase of land on a planning horizon of 21
   years. In the real options analysis, we used deferral, expansion, and
   abandonment. Results: Individually, the deferral, expansion, and
   abandonment options add value to investment projects in Pinus elliottii
   plantations. The option to expand the forested area is one that adds the
   most value to the investment project with land lease. In the investment
   project with land purchase, it is abandonment. Conclusions: Investment
   projects in Pinus elliotti plantations that contemplate the land
   purchase analyzed through the real options analysis present higher
   financial returns than those that consider land lease, inverting the
   result provided by the traditional analysis.},
Type = {Article},
Affiliation = {da Silva, RBG (Corresponding Author), Sao Paulo State Univ UNESP, Sch Agr, BR-18610034 Botucatu, SP, Brazil.
   Martins, Jorge Carvalho; da Silva, Richardson Barbosa Gomes; Munis, Rafaele Almeida; Simoes, Danilo, Sao Paulo State Univ UNESP, Sch Agr, BR-18610034 Botucatu, SP, Brazil.},
DOI = {10.3390/f13010111},
Article-Number = {111},
Keywords = {biological assets; managerial flexibility; forest management; binomial
   model; silvicultural practices; net present value},
Keywords-Plus = {PLANTED FORESTS; CAPACITY CHOICE; VALUATION; UNCERTAINTY; FLEXIBILITY;
