@article{ WOS:000325811600015,
Author = {Chiochetta, Claudete G. and Radetski, Marilice R. and Correa, Albertina
   X. R. and Tischer, Vinicius and Tiepo, Erasmo N. and Radetski, Claudemir
   M.},
Title = {Abandoned coal mining sites: using ecotoxicological tests to support an
   industrial organic sludge amendment},
Journal = {ENVIRONMENTAL SCIENCE AND POLLUTION RESEARCH},
Year = {2013},
Volume = {20},
Number = {11},
Pages = {7656-7665},
Month = {NOV},
Abstract = {The different stages involved in coal mining-related activities result
   in a degraded landscape and sites associated with large amounts of
   dumped waste material. Remediation of these contaminated soils can be
   carried out by application of industrial organic sludge if the concerns
   regarding the potential negative environmental impacts of this
   experimental practice are properly addressed. In this context, the
   objective of this study was to use ecotoxicological tests to determine
   the quantity of organic industrial sludge that is required as a soil
   amendment to restore soil production while avoiding environmental
   impact. Chemical analysis of the solids (industrial sludge and soil) and
   their leachates was carried out as well as a battery of ecotoxicity
   tests on enzymes (hydrolytic activity), bacteria, algae, daphnids,
   earthworms, and higher plants, according to standardized methodologies.
   Solid and leachate samples of coal-contaminated soil were more toxic
   than those of industrial sludge towards enzyme activity, bacteria,
   algae, daphnids, and earthworms. In the case of the higher plants
   (lettuce, corn, wild cabbage, and Surinam cherry) the industrial sludge
   was more toxic than the coal-contaminated soil, and a soil/sludge
   mixture (66:34 \% dry weight basis) had a stimulatory effect on the
   Surinam cherry biomass. The ecotoxicological assessment of the
   coal-contaminated soil remediation using sludge as an amendment is very
   important to determine application rates that could promote a
   stimulatory effect on agronomic species without negatively affecting the
   environment.},
Type = {Article},
Affiliation = {Radetski, C (Corresponding Author), UNIVALI Univ Vale Itajai, Lab Remediacao Ambiental, Rua Uruguai 458, BR-88302202 Itajai, SC, Brazil.
   Chiochetta, Claudete G.; Radetski, Marilice R.; Correa, Albertina X. R.; Tischer, Vinicius; Tiepo, Erasmo N.; Radetski, Claudemir M., UNIVALI Univ Vale Itajai, Lab Remediacao Ambiental, BR-88302202 Itajai, SC, Brazil.},
DOI = {10.1007/s11356-012-1269-2},
Keywords = {Coal mining sites; Acid mine drainage; Soil remediation; Biotests;
