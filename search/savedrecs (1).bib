   Buarque, Angela, Lab Arqueol Brasileira, BR-25251050 Duque De Caxias, Brazil.},
DOI = {10.1007/s00334-013-0397-z},
Keywords = {Landscape; Archaeology; Rainforest; Neotropics; Charcoal; Tupiguarani},
Keywords-Plus = {LATE QUATERNARY VEGETATION; PRISTINE MYTH; SAO-PAULO; HISTORY; CLIMATE;
   STATE; YR; CONSERVATION; EVOLUTION; PATTERNS},
