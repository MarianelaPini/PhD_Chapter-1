@article{ WOS:000432777200002,
Author = {Ribeiro Neto, Nicacio and Ronqui, Raquel Aparecida and Seidinger,
   Leticia Chedid and Udulutsch, Renata Giassi},
Title = {Climbers of the Estacao Ecologica de Assis, State of Sao Paulo, Brazil:
   floristics and identification keys},
Journal = {PHYTOKEYS},
Year = {2018},
Number = {99},
Pages = {67-84},
Month = {MAY 21},
Abstract = {Climbers are an important life form in the understory and canopy of
   tropical forests. They are characterised by constant root connection to
   the ground and use of other species, mainly trees, as support in their
   search for higher light. In addition, they have an important role in
   ecological succession in forest clearings, as they are able to develop
   rapidly in these environments. Climbers can have high species richness
   in the biomes in which they are present. Since climbers are of little
   economic importance, they are usually cut down without regard for their
   contribution to maintenance of biodiversity and to the structure of the
   forest. Floristic studies of climbers in Brazil are still scarce and
   more research is needed. Tine goal of our research was to develop a
   floristic survey and identification keys for the climbers of the Estacao
   Ecologica de Assis (EEA) in the cerrado biome of Sao Paulo state,
   Brazil. Sampling was carried out every two weeks over ten months, along
   trails and edges of forest within the EEA. Identification keys were
   built based on vegetative characters. Thirty-two climber species,
   belonging to 24 genera and 1 3 families were recorded. The families with
   the largest number of species were Bignoniaceae (8 species),
   Malpighiaccae (5), Apocynaceae (3) and Smilacaceae (3). The richest
   genera were Fridericia (4 species), Banisteriopsis (3) and Smilax (3)-
   Tine number of species recorded for the forest fragment reveals the
   important role of climbers in the diversity of forested savannahs
   ({''}cerradoes{''}) in the State of Sao Paulo.},
Type = {Article},
Affiliation = {Udulutsch, RG (Corresponding Author), Univ Estadual Paulista, Fac Ciencias \& Letras Assis, UNESP, BR-19806900 Assis, SP, Brazil.
   Ribeiro Neto, Nicacio; Ronqui, Raquel Aparecida; Udulutsch, Renata Giassi, Univ Estadual Paulista, Fac Ciencias \& Letras Assis, UNESP, BR-19806900 Assis, SP, Brazil.
   Seidinger, Leticia Chedid, Univ Estadual Paulista, Inst Biociencias, UNESP, BR-13506900 Assis, SP, Brazil.},
