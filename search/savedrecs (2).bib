   Intelligenesis Brasil Ltda, BR-30140003 Belo Horizonte, MG, Brazil.},
DOI = {10.1016/S0304-3800(02)00059-5},
Article-Number = {PII S0304-3800(02)00059-5},
Keywords = {land-use and land-cover change; cellular automata; simulation model;
   DINAMICA; Amazonian landscape dynamics},
Keywords-Plus = {SPATIAL MODEL; LAND-USE; FOREST; DEFORESTATION; HIGHLANDS; PATTERNS; GIS},
