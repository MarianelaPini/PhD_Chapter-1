@article{ WOS:000373744900006,
Author = {de Almeida, Gabriela M. A. and Ramos, Marcelo A. and Araujo, Elcida L.
   and Baldauf, Cristina and Albuquerque, Ulysses Paulino},
Title = {Human perceptions of landscape change: The case of a monodominant forest
   of Attalea speciosa Mart ex. Spreng (Northeast Brazil)},
Journal = {AMBIO},
Year = {2016},
Volume = {45},
Number = {4},
Pages = {458-467},
Month = {MAY},
Abstract = {From the perception of human populations, we can assess the changes
   occurring in certain landscapes and the factors that cause those
   changes. Such studies have proven helpful in increasing the knowledge of
   the history of a landscape, recognizing past formations and projecting
   its future. Our research objective was to determine how a landscape
   dominated by the palm tree Attalea speciosa, a species of ecological,
   economic, and cultural importance, has been changing over time by
   synthesizing and comparing historical documents and local perceptions.
   This study was conducted in Araripe Environmental Protection Area,
   Northeast Region, Brazil. To understand local landscape change, we
   interviewed active harvesters in four communities in which A. speciosa
   use has been documented. Historical documents were evaluated as a
   complement to the interview data. According to local informants, areas
   previously used for cultivation and animal husbandry that were abandoned
   or decimated by droughts in the region may have fostered the expansion
   of a monodominant A. speciosa forest. Furthermore, other forms of
   landscape management resulting from human population growth may also
   have affected the current and past distribution of this forest.},
Type = {Article},
Affiliation = {Albuquerque, UP (Corresponding Author), Univ Fed Rural Pernambuco, Lab Etnobiol Aplicada \& Teor LEA, Rua Dom Manoel Medeiros S-N, BR-52171900 Recife, PE, Brazil.
