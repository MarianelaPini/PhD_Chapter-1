@article{ WOS:000429332000014,
Author = {Callegaro, Rafael Marian and Araujo, Maristela Machado and Longhi, Solon
   Jonas and Andrzejewski, Camila},
Title = {Influence of environmental factors on plant species in seasonal forest
   for potential use in restoration},
Journal = {NATIVA},
Year = {2018},
Volume = {6},
Number = {1},
Pages = {91-99},
Month = {FEB},
Abstract = {This study goal was to identify correlations of shrub and tree species
   with environmental variables, in a Deciduous Seasonal Forest in southern
   Brazil. Individuals in the arboreal stratum (DBH >= 5 cm) and natural
   regeneration (Class II: 1 <= DBH < 5 cm; Class II: DBH < 1 cm and height
   >= 20 cm) were sampled. Spearman's coefficient was used to determine the
   correlations. Some species were related to different environmental
   variables as the stage of development. For example, Actinostemon
   concolor and Sorocea bonplandii were tolerant to shallow soils, with
   higher levels of stoniness, a recurrent situation in the forests of the
   Southern Plateau edge, while Cupania vernalis and Nectandra megapotamica
   were correlated negatively to these variables. The zoochoric species are
   important for the formation of the arboreal component and the
   establishment of populations. The different relationships observed show
   that the use of shrub and tree species in ecological restoration should
   be based on the analysis of potentially favorable or limiting
   environmental variables to the establishment of these species.},
Type = {Article},
Affiliation = {Callegaro, RM (Corresponding Author), Univ Fed Espirito Santo, Dept Ciencias Florestais \& Madeira, Jeronimo Monteiro, ES, Brazil.
   Callegaro, Rafael Marian, Univ Fed Espirito Santo, Dept Ciencias Florestais \& Madeira, Jeronimo Monteiro, ES, Brazil.
   Araujo, Maristela Machado; Longhi, Solon Jonas; Andrzejewski, Camila, Univ Fed Santa Maria, Programa Posgrad Engn Florestal, Santa Maria, RS, Brazil.
   Longhi, Solon Jonas, Univ Tecnol Fed Parana, Programa Posgrad Agroecossistemas, Dois Vizinhos, PR, Brazil.},
DOI = {10.31413/nativa.v6i1.4728},
Keywords = {indigenous species; limiting factors; degraded areas; environmental
   recovery},
Keywords-Plus = {GRANDE-DO-SUL; MINAS-GERAIS; BRAZIL; STATE},
Author-Email = {rafaelm.callegaro@gmail.com},
