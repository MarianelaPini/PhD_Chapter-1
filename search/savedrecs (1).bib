@article{ WOS:000401540100010,
Author = {Fragoso, Rosimeri de Oliveira and Carpanezzi, Antonio Aparecido and
   Zuffellato-Ribas, Katia Christina and Koehler, Henrique Soares},
Title = {FORESTRY RESTORATION IN ABANDONED PASTURES OF Urochloa BY
   DIFFERENT SIZES OF BRUSHWOOD},
Journal = {CERNE},
Year = {2017},
Volume = {23},
Number = {1},
Pages = {85-93},
Month = {JAN-MAR},
Abstract = {The brushwood is a technique of environmental complexation, which
   consists in the use of plant residues. This technique, when well
   established technically, can exert influence on seedbed quality,
   fostering native plant establishment and ecosystem restoration. In this
   way, we aimed to evaluate the efficiency of brushwood for the induction
   of natural regeneration of native species in an area covered by exotic
   inhibitory grasses of the genus Urochloa, as well as check the minimum
   size required to prevent the re-invasion of the forage. As a hypothesis,
   we adopted the minimum width of 4 m as sufficient for the establishment
   of natural regeneration prior to the reoccupation of grasses. The
   experiment was conducted between May 2014 and May 2016 in MorretesPR in
   a lowland evergreen rain forest area. Seven different sizes of brushwood
   were compared: 6 x 1, 6 x 2, 6 x 3, 6 x 4, 6 x 5, 6 x 6 m and control
   treatment. Woody species were identified and counted and herbaceous
   cover percentage estimated after 4, 8, 12, 18 and 24 months. There was
   no resumption of natural succession of native species. At any size,
   brushwood alone was ineffective to prevent the growth by grasses from
   the edges of the plots, and, since it constitutes a residue pile, it
   further complicates the establishment of woody species. For the creation
   of safe sites and consequent restoration by natural regeneration, there
   is a need for local elimination of Urochloa forages, without which
   native species are unlikely to survive.},
Type = {Article},
Affiliation = {Fragoso, RD (Corresponding Author), Univ Fed Parana, Curitiba, Parana, Brazil.
   Fragoso, Rosimeri de Oliveira; Zuffellato-Ribas, Katia Christina; Koehler, Henrique Soares, Univ Fed Parana, Curitiba, Parana, Brazil.
   Carpanezzi, Antonio Aparecido, Embrapa Florestas, Colombo, Parana, Brazil.},
DOI = {10.1590/01047760201723012276},
Keywords = {Inhibition; Nucleation; Facilitation; Restoration of degraded
   ecosystems; Natural regeneration},
Keywords-Plus = {ATLANTIC RAIN-FOREST; ECOLOGICAL RESTORATION; SOUTHERN BRAZIL; LITTER;
   TREES; TRANSPOSITION; REGENERATION; NUCLEATION; AUSTRALIA},
Author-Email = {meri\_ol@yahoo.com.br},
Times-Cited = {3},
Unique-ID = {WOS:000401540100010},
DA = {2025-04-03},
}

@article{ WOS:000618184500004,
Author = {de Paula, Felipe Rossetti and Leal, Cecilia Gontijo and Leitao, Rafael
   Pereira and Ferraz, Silvio Frosini de Barros and Pompeu, Paulo Santos
   and Zuanon, Jansen Alfredo Sampaio and Hughes, Robert M.},
Title = {The role of secondary riparian forests for conserving fish assemblages
   in eastern Amazon streams},
Journal = {HYDROBIOLOGIA},
Year = {2022},
Volume = {849},
Number = {20},
Pages = {4529-4546},
Month = {NOV},
Abstract = {Riparian deforestation degrades stream habitats, altering fish
   assemblages' structure, and composition. In contrast, secondary riparian
   forests can recover stream habitats and fish assemblages as they recover
   structural attributes and ecological processes. We evaluated whether the
   amount and condition of secondary riparian forests were important to
   conserve fish assemblages in 49 streams sites in eastern Amazonia. We
   related fish assemblage taxonomic and functional measures to riparian
   forest amount plus different habitat metrics using regression analysis.
   We compared assemblage measures among reference forests, abandoned
   pastures (open canopy), and secondary forests (closed canopy) using
   ANOVA tests. The amount of secondary forests had little influence on
   fish assemblages. Species richness, diversity, and functional richness
   were higher in pasture than in reference sites but returned to
   pre-disturbance conditions in secondary sites. However, functional
   evenness was lower in pasture streams and did not recover after
   secondary forest regrowth. Our results show that secondary riparian
   forest condition is important to recover some aspects of fish
   assemblages. However, streams bordered by these forests may have
   impoverished fish assemblages because some lost sensitive species may
   take longer to return. Avoiding riparian deforestation is the best
   strategy to reduce losses in aquatic biodiversity and ecosystem
   functions in tropical agricultural landscapes.},
