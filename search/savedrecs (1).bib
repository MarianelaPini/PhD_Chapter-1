   Delgado, Rafael Coll, Univ Fed Rural Rio de Janeiro, Inst Florestas, Dept Ciencias Ambientais, Seropedica, RJ, Brazil.
   Gleriani, Jose Marinaldo, Univ Fed Vicosa, Dept Engn Florestal, Vicosa, MG, Brazil.},
DOI = {10.1590/0100-67622016000500004},
Keywords = {Resilience; Forest fragmentation; Remote sensing},
