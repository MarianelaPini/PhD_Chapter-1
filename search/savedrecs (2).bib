DA = {2025-04-03},
}

@article{ WOS:001207756900001,
Author = {Vilela, Herminio A. L. S. and Andrade, Arthur B. and Araujo, Helder F.
   P. and Bichinski, Tony A. and Cavalcante, Barbara and Develey, Pedro F.
   and Reisfeld, Alice and Phalan, Benjamin T.},
Title = {The Alagoas Antwren Myrmotherula snowi is in imminent danger of
   extinction},
Journal = {BIRD CONSERVATION INTERNATIONAL},
Year = {2024},
Volume = {34},
Month = {APR 25},
Abstract = {The ``Critically Endangered{''} Alagoas Antwren Myrmotherula snowi is
   found only in the Atlantic Rainforest of north-east Brazil. We surveyed
   all sites where the species has been recorded, as well as 15 other
   potential sites. We summarise all observations of the species, focusing
   on the period from 2016 onwards, and including systematic censuses in
   three breeding seasons (October-March 2018/19, 2019/20, and 2020/21) at
   the type locality, Murici, where we also searched for nests of the
   species. We did not locate any individuals at sites other than Murici.
   The number of apparent individuals detected declined by 67\% over seven
   years (a decline of 50\% over the three years in which standardised
   systematic counts were made), and only six adults were observed in the
   most recent breeding season. The global population is now likely to be
   in single-digit figures. Of five nests found, four failed, through
   predation by snakes and opossums, and abandonment. We installed
   protective barriers to protect two of these nests from predators, which
   may have contributed to the single successful fledging. Such methods
   offer most hope for now of stalling the decline. Efforts to develop
   captive breeding methods with model species have been underway since
   2019 but face many difficulties. Improving habitat protection and
   restoration is crucial but will only take effect in the long term.},
Type = {Article},
Affiliation = {Phalan, BT (Corresponding Author), Ctr Conservat Atlantic Forest Birds, Inst Claravis, Parque Aves, Foz Do Iguacu, Brazil.
   Vilela, Herminio A. L. S.; Andrade, Arthur B.; Bichinski, Tony A.; Cavalcante, Barbara; Develey, Pedro F.; Reisfeld, Alice, SAVE Brasil BirdLife Brazil, Sao Paulo, Brazil.
   Araujo, Helder F. P., Univ Fed Paraiba, Joao Pessoa, PB, Brazil.
   Bichinski, Tony A.; Phalan, Benjamin T., Ctr Conservat Atlantic Forest Birds, Inst Claravis, Parque Aves, Foz Do Iguacu, Brazil.},
DOI = {10.1017/S0959270924000078},
Article-Number = {e12},
Keywords = {Bird population decline; Brazilian Atlantic Rainforest; Thamnophilidae;
   Threatened species},
Author-Email = {benjamin@parquedasaves.com.br},
Times-Cited = {1},
Unique-ID = {WOS:001207756900001},
DA = {2025-04-03},
}

@article{ WOS:000335430000001,
Author = {Notaro, Krystal de Alcantara and de Medeiros, Erika Valente and Duda,
   Gustavo Pereira and Silva, Aline Oliveira and de Moura, Patricia Maia},
Title = {Agroforestry systems, nutrients in litter and microbial activity in
   soils cultivated with coffee at high altitude},
Journal = {SCIENTIA AGRICOLA},
Year = {2014},
Volume = {71},
Number = {2},
Pages = {87-95},
Month = {MAR-APR},
Abstract = {Agroforestry systems are an alternative option for sustainable
   production management. These systems contain trees that absorb nutrients
   from deeper layers of the soil and leaf litter that help improve the
   soil quality of the rough terrain in high altitude areas, which are
   areas extremely susceptible to environmental degradation. The aim of
   this study was to characterize the stock and nutrients in litter, soil
   activity and the population of microorganisms in coffee (Coffea arabica
   L.) plantations under high altitude agroforestry systems in the
   semi-arid region of the state of Pernambuco, Brazil. Samples were
   collected from the surface litter together with soil samples taken at
   two depths (0-10 and 10-20 cm) from areas each subject to one of the
   following four treatments: agroforestry system (AS), native forest (NF),
   biodynamic system (BS) and coffee control (CT). The coffee plantation
   had been abandoned for nearly 15 years and, although there had been no
   management or harvesting, still contained productive coffee plants. The
   accumulation of litter and mean nutrient content of the litter, the soil
   nutrient content, microbial biomass carbon, total carbon, total
   nitrogen, C/N ratio, basal respiration, microbial quotient, metabolic
   quotient and microbial populations (total bacteria, fluorescent bacteria
   group, total fungi and Trichoderma spp.) were all analyzed. The systems
   that were exposed to human intervention (AS and BS) differed in their
   chemical attributes and contained higher levels of nutrients when
   compared to NF and CT. BS for coffee production at high altitude can be
   used as a sustainable alternative in the high altitude zones of the
   semi-arid region in Brazil, which is an area that is highly susceptible
   to environmental degradation.},
Type = {Article},
Affiliation = {Duda, GP (Corresponding Author), Univ Fed Rural Pernambuco, Acad Unit Garanhuns, Av Bom Pastor S-N, BR-55292270 Garanhuns, PE, Brazil.
   Notaro, Krystal de Alcantara; de Medeiros, Erika Valente; Duda, Gustavo Pereira; Silva, Aline Oliveira; de Moura, Patricia Maia, Univ Fed Rural Pernambuco, Acad Unit Garanhuns, BR-55292270 Garanhuns, PE, Brazil.},
Keywords-Plus = {CARBON SEQUESTRATION; ENZYME-ACTIVITIES; ORGANIC-CARBON; QUALITY;
   BIOMASS; MANAGEMENT; NITROGEN; FOREST},
Author-Email = {gpduda@gmail.com},
Times-Cited = {38},
Unique-ID = {WOS:000335430000001},
DA = {2025-04-03},
}

@article{ WOS:000072028000001,
Author = {Faminow, MD},
Title = {The Brazilian cattle sector: Status, prospects and controversies},
Journal = {CANADIAN JOURNAL OF AGRICULTURAL ECONOMICS-REVUE CANADIENNE D
   AGROECONOMIE},
Year = {1997},
Volume = {45},
Number = {3},
Pages = {179-199},
Month = {NOV},
Abstract = {The cattle sector in Brazil (and in other countries of South America) is
   substantial in terms of size and potential impacts in world markers. Due
   to the sheer magnitude and potential of the Brazilian cattle sector, it
   merits out. attention. A trend to more productive cattle systems, new
   rules for trade in livestock products and more effective control of hoof
   and mouth disease in some ar eas of Brazil could cause it to become a
   more formidable beef importer. Brazil also warrants attention because of
   the controversial practice of deforestation for cattle production in the
   Amazon rain forest. A vocal environmental lobby has decried cattle
   production in the Amazon, but this practice continues. This paper
   reviews the status, prospects and controversies of the beef sector in
   Brazil, with a special focus on deforestation and cattle production in
   tropical ecosystems.},
Type = {Article},
Affiliation = {Univ Manitoba, Dept Agr Econ \& Farm Management, Winnipeg, MB R3T 2N2, Canada.
   Univ Manitoba, Dept Agr Econ \& Farm Management, Winnipeg, MB R3T 2N2, Canada.},
