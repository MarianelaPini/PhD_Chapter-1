   Silva Junior, Juvenal Cordeiro, Univ Estadual Sudoeste Bahia, BR-45208091 Jequie, BA, Brazil.},
DOI = {10.22571/2526-4338415},
Keywords = {Fungivory; insect diversity; leaf-litter; microenvironment; soil fauna},
Keywords-Plus = {SPECIES-DIVERSITY; ANT DIVERSITY; DECOMPOSITION; COMMUNITIES; RECOVERY},
Author-Email = {miridovalle@hotmail.com},
