@article{ WOS:000460704900013,
Author = {dos Reis-Neto, Armando Soares and de Andrade Meireles, Antonio Jeovah
   and Cunha-Lignon, Marilia},
Title = {Natural Regeneration of the Mangrove Vegetation on Abandoned Salt Ponds
   in Ceara, in the Semi-Arid Region of Northeastern Brazil},
Journal = {DIVERSITY-BASEL},
Year = {2019},
Volume = {11},
Number = {2},
Month = {FEB},
Abstract = {The development of the mangrove in Ceara state, northeastern Brazil, is
   limited by local environmental and climatic factors, associated with the
   variables that determine the region's semi-arid climatic conditions. The
   same conditions also contribute to the installation of artisanal
   saltworks in estuarine environments. The artisanal production of salt
   peaked in the 20th century, but with the decline of this activity, the
   salt evaporation ponds were abandoned, and have been incorporated back
   into the natural marine-estuarine environment and colonized by mangrove
   forests. In the early 2000s, however, the expansion of shrimp farming
   operations impacted this same environment. The present study was based
   on a spatiotemporal analysis of the natural regeneration of the mangrove
   vegetation in abandoned salt pond areas in the Brazilian semi-arid
   region between 1968 and 2009. The integrated analysis of mangrove
   ecosystem dynamics and the legislation that regulates the licensing of
   these economic activities identified a number of technical problems in
   the formulation and execution of the COEMA resolution 02/2002, which
   permits the installation of shrimp farms in areas dominated by the
   mangrove. The findings of the present study reinforce the need for a
   careful reformulation of the Ceara state environmental legislation, in
   order to guarantee the maximum possible conservation of the coastal
   zone.},
Type = {Article},
Affiliation = {dos Reis-Neto, AS (Corresponding Author), Univ Sao Paulo, Grad Program Environm Sci PROCAM, Energy \& Environm Inst, BR-05508010 Sao Paulo, SP, Brazil.
   dos Reis-Neto, Armando Soares, Univ Sao Paulo, Grad Program Environm Sci PROCAM, Energy \& Environm Inst, BR-05508010 Sao Paulo, SP, Brazil.
