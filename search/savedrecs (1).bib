Keywords = {Formicidae; Interspecific Competition; Foraging Activity},
Keywords-Plus = {RAIN-FOREST; TRADE-OFF; DISCOVERY; FAUNA; COMMUNITIES; DOMINANCE},
Author-Email = {lc.santosjunior@yahoo.com.br},
