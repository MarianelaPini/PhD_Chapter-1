   Moulatlet, Gabriel M., Univ Turku, Dept Biol, Turku, Finland.},
DOI = {10.1016/j.catena.2017.06.022},
Keywords = {Forest canopy; Landscape evolution; Mid/late Holocene; Tectonics;
   Northern Amazonia},
Keywords-Plus = {LATE QUATERNARY EVOLUTION; WHITE-SAND VEGETATION; SEDIMENTARY PROCESSES;
