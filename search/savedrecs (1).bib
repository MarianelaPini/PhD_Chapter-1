@article{ WOS:001157676500001,
Author = {Oliveira, Priscila S. and Falcao, Luiz A. D. and Almeida, Jarcilene S.
   and Fernandes, Geraldo Wilson and Reis Jr, Ronaldo and Nunes, Yule R. F.
   and Veloso, Maria das Dores M. and Beirao, Marina do Vale and Neves,
   Frederico de Siqueira and Solar, Ricardo R. C. and Borges, Magno A. Z.
   and Silva, Alex C. and Salomao, Renato P. and Iannuzzi, Luciana and
   Silva, Luciana F. and Cabral, George A. L. and Sampaio, Everardo V. S.
   B. and Macedo-Reis, Luiz E. and Santos, Cleandson F. and Kerpel, Solange
   M. and Souza, Rayana M. and Cardoso, Iaciara G. S. and Santo, Mario M.
   do Espirito},
Title = {Diversity patterns along ecological succession in tropical dry forests:
   a multi-taxonomic approach},
Journal = {OIKOS},
Year = {2024},
Volume = {2024},
Number = {4},
Month = {APR},
Abstract = {Evaluating the diversity of multiple taxa is fundamental to understand
   community assembly and to assess the integrity and functionality of
   tropical secondary forests. In this study, we analyzed the natural
   regeneration of tropical dry forests (TDFs) in three regions of Brazil
   using Hill-Simpson diversity, abundance and beta-diversity of trees and
   five groups of insects (herbivores, fruit-feeding butterflies, ants,
   culicid mosquitoes and dung beetles). Sampling was conducted in 39 0.1
   ha plots using a chronosequence approach (13 plots in early,
   intermediate and old-growth forests). We evaluated the contribution of
   three different levels to gamma-diversity: alpha (within plots), beta 1
   (among plots) and beta 2 (among successional stages), and further
   determined the relative importance of turnover (species replacement) and
   nestedness (differences in species number among sites) to beta 2. Our
   results showed that, unexpectedly, the Hill-Simpson diversity was
   consistently higher in early than old-growth stages for all regions, but
   varied more widely in the intermediate stages. For each group
   separately, the same trend was observed for butterflies, ants, dung
   beetles and herbivores and did not differ among stages for mosquitoes.
   Successional differences in abundance were only detected for trees
   (increasing along the gradient) and for mosquitoes (decreasing).
   According to our expectations, the additive partitioning analysis showed
   that beta 2-diversity contributed more to gamma-diversity than beta
   1-diversity, when all taxa were considered together and for most of them
   separately (except for butterflies and dung beetles). Most of the beta
   2-diversity was due to species turnover, but this contribution varied
   among groups and regions, with the highest turnover for herbivores and
   the lowest for dung beetles. Our results suggest that the Hill-Simpson
   diversity and changes in species composition (as given by beta
   2-diversity) are better indicators of forest natural regeneration than
   raw species richness, corroborating previous studies with plants and
   animals.},
Type = {Article},
Affiliation = {Santo, MMD (Corresponding Author), Univ Estadual Montes Claros, Montes Claros, MG, Brazil.
   Oliveira, Priscila S.; Falcao, Luiz A. D.; Reis Jr, Ronaldo; Nunes, Yule R. F.; Veloso, Maria das Dores M.; Borges, Magno A. Z.; Silva, Luciana F.; Santos, Cleandson F.; Cardoso, Iaciara G. S.; Santo, Mario M. do Espirito, Univ Estadual Montes Claros, Montes Claros, MG, Brazil.
