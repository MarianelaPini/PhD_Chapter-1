@article{ WOS:000175610900011,
Author = {Godsey, S and Elsenbeer, H},
Title = {The soil hydrologic response to forest regrowth: a case study from
   southwestern Amazonia},
Journal = {HYDROLOGICAL PROCESSES},
Year = {2002},
Volume = {16},
Number = {7},
Pages = {1519-1522},
Month = {MAY},
Abstract = {As a large and dynamic land-use category, tropical secondary forests may
   affect climate, soils, and hydrology in a manner different from primary
   forests or agricultural areas. We investigated the saturated hydraulic
   conductivity K-sat of a Kandiudult under different land uses in
   Rondonia, Brazil. We measured K-sat at four depths (12.5, 20, 30 and 50
   cm) under (a) primary forest, (b) a former banana-cacao plantation
   (SF1), and (c) an abandoned pasture (SF2). At 12.5 cm, all three land
   uses differ significantly (alpha = 0.1), but not at the 20 and 30 cm
   depths. At 50 cm, K-sat was significantly greater in the former pasture
   than in other land uses. Lateral subsurface flow is expected during
   intense rainfall (about 30 times per year) at 30 cm depth in SF1 and at
   50 em depth in the forest, whereas the relatively low permeability at
   shallow 12.5 cm in the SF2 may result not only in lateral subsurface
   flow, but also saturation overland flow. For modelling purposes,
   recovering systems seem to have Ksat values distinct from primary forest
   at shallow depths, whereas at deeper layers (>20 cm) they may be
   considered similar to forests. Copyright (C) 2002 John Wiley Sons, Ltd.},
Type = {Article},
Affiliation = {Godsey, S (Corresponding Author), Univ Cincinnati, Dept Civil \& Environm Engn, Cincinnati, OH 45221 USA.
   Univ Cincinnati, Dept Civil \& Environm Engn, Cincinnati, OH 45221 USA.},
DOI = {10.1002/hyp.605},
Keywords = {secondary forest; saturated hydraulic conductivity; land
   parameterization; hydrological flowpaths; Amazon basin},
Keywords-Plus = {TROPICAL FOREST; DEFORESTATION; REGENERATION; RECOVERY; PASTURES;
   RONDONIA; BRAZIL},
Times-Cited = {57},
