DOI = {10.1590/2317-1545v39n1168605},
Keywords = {serotiny; seed longevity; seed vigor; angico-amarelo},
Keywords-Plus = {MEAN GERMINATION TIME; STORAGE; EMBRYOGENESIS; DESICCATION; TEMPERATURE;
   EXPRESSION; TOLERANCE; EMERGENCE; PROTEIN; FOREST},
