@article{ WOS:000251059500003,
Author = {Juang, Jehn-Yih and Katul, Gabriel and Siqueira, Mario and Stoy, Paul
   and Novick, Kimberly},
Title = {Separating the effects of albedo from eco-physiological changes on
   surface temperature along a successional chronosequence in the
   southeastern United States},
Journal = {GEOPHYSICAL RESEARCH LETTERS},
Year = {2007},
Volume = {34},
Number = {21},
Month = {NOV 15},
Abstract = {In the southeastern United States (SE), the conversion of abandoned
   agricultural land to forests is the dominant feature of land-cover
   change. However, few attempts have been made to quantify the impact of
   such conversion on surface temperature. Here, this issue is explored
   experimentally and analytically in three adjacent ecosystems (a
   grass-covered old-field, OF, a planted pine forest, PP, and a hardwood
   forest, HW) representing a successional chronosequence in the SE. The
   results showed that changes in albedo alone can warm the surface by 0.9
   degrees C for the OF-to-PP conversion, and 0.7 degrees C for the
   OF-to-HW conversion on annual time scales. However, changes in
   eco-physiological and aerodynamic attributes alone can cool the surface
   by 2.9 and 2.1 degrees C, respectively. Both model and measurements
   consistently suggest a stronger over-all surface cooling for the
   OF-to-PP conversion, and the reason is attributed to leaf area
   variations and its impacts on boundary layer conductance.},
Type = {Article},
Affiliation = {Juang, JY (Corresponding Author), Duke Univ, Nicholas Sch Environm \& Earth Sci, Durham, NC 27706 USA.
   Duke Univ, Nicholas Sch Environm \& Earth Sci, Durham, NC 27706 USA.
