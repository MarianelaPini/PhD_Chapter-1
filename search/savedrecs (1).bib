@article{ WOS:000296689500005,
Author = {de Avila, Angela Luciana and Araujo, Maristela Machado and Longhi, Solon
   Jonas and Gasparin, Ezequiel},
Title = {Floristic clusters on natural regeneration in Mixed Rain Forest remnant,
   RS, Brazil},
Journal = {SCIENTIA FORESTALIS},
Year = {2011},
Volume = {39},
Number = {91},
Pages = {331-342},
Month = {SEP},
Abstract = {This research aimed to identify the presence of floristic clusters in
   the mechanisms of natural regeneration and the influence of
   environmental factors on the distribution of species and plots in the
   seedling bank. The study was conducted in the National Forest of Sao
   Francisco de Paula, RS, using six conglomerates (1ha), with 16 plots (20
   x 20m) each, disregarding the surrounding 10m. Seed rain (SR) was
   sampled in 96 circular collectors (1m(2)), distributed in the center of
   each plot. The soil seed bank (SB) was collected, using iron templates
   (0.25 x 0.25 m) and 3m of each collector from CS. The seedling bank
   (SeB) was sampled in plots of 2 x 2m, located 4m from SR collectors,
   counting the number of individuals per specie, with height >= 30cm and
   diameter at breast height (DBH) < 1cm. Cluster analysis was performed by
   TWINSPAN (Two-way Indicator Species Analysis), associated with canonical
   correspondence analysis (CCA) for BP. The presence of the CS and BS
   clusters was verified by using the nonparametric test of the sum of the
   orders (Wilcoxon). Three groups were identified and CCA indicated the
   slope as the environmental variable with the greatest influence on the
   distribution of species and plots. The groups showed great relationship
   with its history of use; with the highest densities and lowest richness
   for the three mechanisms in the group related with the highest level of
   human interference. The presence of clusters indicates that the
   regenerative processes of the forest do not occur equally in the
   remaining forest; is necessary to know them to enable the planning of
   conservation and management strategies, appropriate to each species and
   the environment.},
Type = {Article},
Affiliation = {de Avila, AL (Corresponding Author), Univ Fed Santa Maria, Programa Posgrad Engn Florestal, Ctr Ciencias Rurais, BR-97105900 Santa Maria, RS, Brazil.
   de Avila, Angela Luciana; Gasparin, Ezequiel, Univ Fed Santa Maria, Programa Posgrad Engn Florestal, Ctr Ciencias Rurais, BR-97105900 Santa Maria, RS, Brazil.
