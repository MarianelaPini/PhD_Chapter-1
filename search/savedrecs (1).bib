   Tabarelli, Marcelo; Leal, Inara R., Univ Fed Pernambuco, Dept Bot, Ave Prof Moraes Rego S-No,Cidade Univ, BR-50670901 Recife, PE, Brazil.},
DOI = {10.1111/btp.12782},
EarlyAccessDate = {MAR 2020},
Keywords = {Atta opaciceps; Brazil; foraging activity; forest cover; herbaceous
   plants; plant recruitment; seasonally dry tropical forest; woody plants},
