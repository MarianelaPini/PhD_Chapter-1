DOI = {10.1016/j.foreco.2017.04.018},
Keywords = {Abandoned pastures; Ecological restoration; Land use; Natural
   regeneration; Secondary forests; Vismia},
Keywords-Plus = {TREE SEEDLING RECRUITMENT; ABOVEGROUND BIOMASS; CANOPY GAP; RAIN;
   REGENERATION; SUCCESSION; VEGETATION; LAND; RESILIENCE; PATTERNS},
Author-Email = {vbtonyco@gmail.com},
