DOI = {10.1016/j.ecolmodel.2025.111089},
Article-Number = {111089},
Keywords = {Tropical saltmarsh; Water balance model; Salinity; Eco-hydrological
   model},
Keywords-Plus = {SECONDARY SUCCESSION; MANGROVES; VEGETATION; PATTERNS; TRACER},
Author-Email = {ronny.peters@tu-dresden.de},
