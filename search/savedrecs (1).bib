DOI = {10.1016/j.foreco.2011.03.001},
Keywords = {Eucalyptus; Natural regeneration; Seed germination},
Keywords-Plus = {PINUS-RADIATA; REGENERATION; HERBIVORY; RECRUITMENT; COMPETITION;
   GLOBULUS; INVASION},
