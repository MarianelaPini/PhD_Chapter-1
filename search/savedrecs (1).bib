Keywords = {Amazonia; catastrophic disturbance; light-demanding non-pioneer;
   mahogany; natural regeneration; Swietenia},
Keywords-Plus = {RAIN-FOREST; ROOT COMPETITION; SOUTHERN NIGERIA; PLATEAU FOREST;
   AMAZONIA; KING; CONSERVATION; DISTURBANCE; BRAZIL; OKOMU},
