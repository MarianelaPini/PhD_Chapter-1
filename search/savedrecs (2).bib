   Moran, Emilio F., Michigan State Univ, Ctr Global Change \& Earth Observat, E Lansing, MI 48823 USA.},
DOI = {10.3390/land7040125},
Article-Number = {125},
Keywords = {rural property survey; thompson tau technique; land-use decisions;
   Atlantic forest transition},
Keywords-Plus = {LAND-USE CHANGE; DYNAMICS; CONSEQUENCES; ABANDONMENT; MIGRATION;
   DRIVERS; VALLEY; COVER},
Author-Email = {ramonbicudo@gmail.com
