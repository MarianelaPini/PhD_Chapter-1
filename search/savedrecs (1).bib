@article{ WOS:000387775800010,
Author = {Schroder, Thomas and Lora Zimmermann, Anna Paula and Pereira, Llian
   Daniel and Ananias Hofico, Noe dos Santos and Rocha Junior, Dilson Sousa
   and Fleig, Frederico Dimas and Meyer, Evandro Alcir},
Title = {USING BARE-ROOT AND ROOTSTOCK SEEDLINGS FROM NATURAL REGENERATION OF
   CEDRELA FISSILIS},
Journal = {CERNE},
Year = {2016},
Volume = {22},
Number = {3},
Pages = {309-315},
Month = {JUL-SEP},
Abstract = {The success of forest creation relies on seedling availability and
   quality. The aim of this research is to assess the possible use of
   rootstock and bare-root seedlings of Cedrela fissilis obtained from
   natural regeneration for direct field planting. We used Generalized
   Linear models to assess survival and Hierarchical Models to assess
   height growth over one year after planting. Initial root-collar diameter
   (RCD) and treatment (rootstock or bare-root) affected survival and
   growth. Rootstock and larger seedlings presented higher levels of
   survival and growth. Higher seedling quality in the rootstock treatment
   is due to higher water stress tolerance. Rootstock seedlings with more
   than 1 cm of initial RCD had over 80\% of survival probability, while
   small bare-root seedlings had mean survival probability as low as 20\%.
   Rootstock seedlings grew as much as fourfold more than bare-root. Using
   natural regeneration of C. fissilis as a source of rootstock seedlings
   may be a cheap alternative for forest restoration and enrichment
   planting projects.},
Type = {Article},
Affiliation = {Schroder, T (Corresponding Author), Univ Fed Santa Maria, Santa Maria, RS, Brazil.
   Schroder, Thomas; Lora Zimmermann, Anna Paula; Pereira, Llian Daniel; Fleig, Frederico Dimas; Meyer, Evandro Alcir, Univ Fed Santa Maria, Santa Maria, RS, Brazil.
   Ananias Hofico, Noe dos Santos, Univ Zambeze, Beira, Sofala, Mozambique.
   Rocha Junior, Dilson Sousa, Univ Fed Bahia, Vitoria De Conquista, BA, Brazil.},
DOI = {10.1590/01047760201622032203},
Keywords = {Tree planting technique; Growth; Survival; Wildlings},
Keywords-Plus = {SILVICULTURAL TREATMENTS; INTENSIVE MANAGEMENT; LOGGING GAPS; GROWTH;
