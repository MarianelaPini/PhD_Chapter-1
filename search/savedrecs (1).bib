Affiliation = {dos Santos, R (Corresponding Author), Univ Extremo Sul Catarinense, Herbario Pe Dr Raulino Reitz,Av Univ 1105, BR-88806000 Criciuma, SC, Brazil.
   dos Santos, Robson; Citadini-Zanette, Vanilde, Univ Extremo Sul Catarinense, BR-88806000 Criciuma, SC, Brazil.
   Leal-Filho, Laurindo S.; Hennies, Wildor T., Univ Sao Paulo, Escola Politecn, Dept Engn Minas \& Petroleo, BR-05508900 Sao Paulo, Brazil.},
DOI = {10.1111/j.1526-100X.2007.00309.x},
