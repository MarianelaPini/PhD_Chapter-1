@article{ WOS:000248619000013,
Author = {Sampaio, Alexandre B. and Holl, Karen D. and Scariot, Aldicir},
Title = {Does restoration enhance regeneration of seasonal deciduous forests in
   pastures in central Brazil?},
Journal = {RESTORATION ECOLOGY},
Year = {2007},
Volume = {15},
Number = {3},
Pages = {462-471},
Month = {SEP},
Abstract = {The goal of restoration is to accelerate ecosystem recovery, but in
   ecosystems that naturally regenerate rapidly restoration techniques need
   to be selected carefully to facilitate rather than impede natural
   recovery. We compared the effects of five restoration techniques, such
   as plowing the soil, removing grasses, adding forest litter, seeding,
   and planting nursery-growing seedlings, on the regeneration of seasonal
   deciduous forest trees in four abandoned pastures in central Brazil. We
   monitored all woody stems immediately prior to treatments and again 14
   months after the treatments. We recorded an average of 16,663 tree stems
   per hectare and a total of 83 species before implementing treatments.
   Planting strongly increased species richness; adding litter and seeding
   had weaker positive effects on richness; and plowing and grass removal
   had no effect. Plowing substantially reduced the density of naturally
   established stems. Despite the high survival of planted seedlings, stem
   density in planting treatments did not change because the tractor and
   digging holes to plant seedlings caused mortality of naturally
   regenerating seedlings. Tree stems grew more in the grass release plots
   than in the control plots. Our results suggest that early succession of
   seasonal deciduous forest in pastures in the region studied does not
   need to be stimulated once the perturbation is stopped and that
   intensive restoration efforts may actually slow recovery. We recommend
   only enrichment planting of seedlings that are not able to resprout.},
Type = {Article},
Affiliation = {Sampaio, AB (Corresponding Author), IBAMA, SCEN Trecho 2, Ed sede DIREC,Cx Postal 09870, BR-70818900 Brasilia, DF, Brazil.
   Univ Brasilia, Post Grad Ecol, BR-70919970 Brasilia, DF, Brazil.
   Ed Corporate Financial Ctr, United Nat Dev Programme, BR-70712901 Brasilia, DF, Brazil.},
DOI = {10.1111/j.1526-100X.2007.00242.x},
Keywords = {cerrado biome; colonization; Coppice shooting; dry forest; forest
   succession; pasture; regrowth; resilience; resprout},
Keywords-Plus = {ABANDONED PASTURES; RAIN-FOREST; VEGETATION RECRUITMENT; NATURAL
   REGENERATION; TROPICAL FORESTS; TREE SEEDLINGS; DRY FOREST; SEED RAIN;
   SUCCESSION; RECOVERY},
Author-Email = {alex\_bonesso@yahoo.com.br},
Times-Cited = {68},
