@article{ WOS:000752356000004,
Author = {Marcelini, Diego Pereira and Reis, Miguel Gama and Fortes, Elenice
   Aparecida and Andrade, Renon Santos and Ferreira Junior, Walnir Gomes},
Title = {Floristics and phytosociology of a recovered stretch of riparian forest
   on the Rio Machado, Minas Gerais},
Journal = {REVISTA AGROGEOAMBIENTAL},
Year = {2021},
Volume = {13},
Number = {3},
Pages = {543-557},
Month = {SEP},
Abstract = {Riparian forests have an important role in the maintenance of
   environmental services and in the conservation of species, playing the
   role of genetic corridor for flora and fauna. The present study was
   conducted with the objective of analyzing the species composition and
   horizontal structure in a restored riparian forest with an adjacent
   native area, in the municipality of Poco Fundo, Minas Gerais. A
   floristic and phytosociological survey of the tree sinusia was carried
   out in the areas. We sampled 199 individuals and 24 species in the
   restored area and 105 individuals and 23 species in the native area. No
   floristic similarity was verified between the areas. Shannon diversity
   was low in both areas. In the restored area, there is a predominance of
   early species while in the native area there is a predominance of late
