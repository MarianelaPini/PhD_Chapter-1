Affiliation = {Lacerda, AEB (Corresponding Author), Embrapa Forestry, Estr Ribeira,Km 111,Caixa Postal 319, BR-83411000 Colombo, PR, Brazil.
   Lacerda, Andre Eduardo Biscaia, Embrapa Forestry, Estr Ribeira,Km 111,Caixa Postal 319, BR-83411000 Colombo, PR, Brazil.},
DOI = {10.3390/conservation3030027},
Keywords = {traditional ecological knowledge; forest management; natural
   regeneration; forest restoration; agroforestry},
Keywords-Plus = {BAMBOOS},
Author-Email = {andre.biscaia@embrapa.br},
