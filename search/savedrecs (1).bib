Keywords = {Histosols; urban soils; subsidence; acid sulfate soils},
Keywords-Plus = {LABILE ORGANIC-CARBON; BLACK CARBON; PEAT SOILS; MANAGEMENT; FRACTIONS;
   PEATLANDS; NITROGEN; ECOLOGY},
Author-Email = {mgervasiopereira01@gmail.com},
