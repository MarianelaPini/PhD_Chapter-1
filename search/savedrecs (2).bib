@article{ WOS:000948015600001,
Author = {Rocha, Quinny Soares and Munis, Rafaele Almeida and Gomes da Silva,
   Richardson Barbosa and Zavaleta Aguilar, Eli Wilfredo and Simoes, Danilo},
Title = {Photovoltaic Solar Energy in Forest Nurseries: A Strategic Decision
   Based on Real Options Analysis},
Journal = {SUSTAINABILITY},
Year = {2023},
Volume = {15},
Number = {5},
Month = {MAR},
Abstract = {With the growing demand for investment projects in renewable energy, it
   is essential for the economic feasibility analysis to consider the
   inherent uncertainties of these projects and enable more accurate
   investment decisions. In this way, we analyze whether investment
   projects in photovoltaic panels to produce electrical energy in a forest
   nursery are economically viable through the analysis of real options. We
   consider the energy demand of a forest nursery with an initial
   investment of USD 90,417 in photovoltaic panels. The managerial
   flexibilities of deferral, expansion of 30.0\% of the energy production
   capacity, and the project's abandonment were inserted into the binomial
   model of the decision tree, built in discrete time. The project's
   expanded net present value was 79.2\% higher than the traditional net
   present value, capturing the value of flexibilities for managers. The
   deferral option was the one that most impacted the expanded net present
   value. Investment projects in photovoltaic panels to produce electricity
   in a forest nursery are economically viable when analyzed through real
   options.},
Type = {Article},
Affiliation = {Simoes, D (Corresponding Author), Sao Paulo State Univ Unesp, Sch Agr, BR-18610034 Botucatu, Brazil.
   Rocha, Quinny Soares; Munis, Rafaele Almeida; Gomes da Silva, Richardson Barbosa; Simoes, Danilo, Sao Paulo State Univ Unesp, Sch Agr, BR-18610034 Botucatu, Brazil.
   Zavaleta Aguilar, Eli Wilfredo, Sao Paulo State Univ Unesp, Campus Itapeva, BR-18409010 Itapeva, Brazil.},
DOI = {10.3390/su15053960},
Article-Number = {3960},
Keywords = {renewable energy; photovoltaic panels; net present value; managerial
   flexibilities; Eucalyptus; cuttings},
