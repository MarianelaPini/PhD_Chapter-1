DOI = {10.1098/rsos.201855},
Article-Number = {201855},
Keywords = {historical ecology; social-ecological systems; novel ecosystems;
   historical geography; charcoal production; landscape transformation},
Keywords-Plus = {BRANCA STATE-PARK; ENVIRONMENTAL HISTORY},
Author-Email = {alexandrosol@gmail.com},
Times-Cited = {4},
Unique-ID = {WOS:000670406400001},
