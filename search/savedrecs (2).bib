@article{ WOS:000348590200011,
Author = {Marquez Pina-Rodrigues, Fatima Conceicao and Aoki, Juliana},
Title = {SEED RAIN AS INDICATOR OF FOREST FRAGMENT CONSERVATION IN SOROCABA, SAO
   PAULO STATE},
Journal = {CIENCIA FLORESTAL},
Year = {2014},
Volume = {24},
Number = {4},
Pages = {911-923},
Month = {OCT-DEC},
Abstract = {The aim of this study was to analyze the degradation state of forest
   fragments applying seed rain as an indicator. Fragments (n = 5) in
   semideciduous forest, in Sorocaba, SP state, were studied from August to
   October, in 2007, and from January to December, in 2008, and seed traps
   were established in 11 plots (10 x 10 m). The material was separated and
   seeds and fruits were identified and classified based on dispersion
   syndrome and habit. Although fragments F4 and F5 showed high richness,
   diversity and equitability, there were dominance of herbaceous and
   anemochoric species. The highest proportion of seed rain was observed in
   F1, F2 and F4, with 36.8\%, 26.8\% and 17.8\%, and F3 and F5, showed
   only 12.6\% and 5.9\%. The seed rain was seasonal and higher in the
   transition from dry to wet season, with 63\% of total seed rain. The
   anemochory was the most representative syndrome (86\%), that is more
   observed in open and degraded areas. The results indicated viability of
   the quantitative analysis of seed rain for monitoring the fragment
   restoration, being effective to detect environmental differences among
   fragments.},
Type = {Article},
Affiliation = {Piña-Rodrigues, FCM (Corresponding Author), Univ Fed Sao Carlos, Dept Ciencias Ambientais, Rod Joao Leme Santos SP 264,Km 110, BR-18052780 Sorocaba, SP, Brazil.
   Marquez Pina-Rodrigues, Fatima Conceicao, Univ Fed Sao Carlos, Dept Ciencias Ambientais, BR-18052780 Sorocaba, SP, Brazil.
