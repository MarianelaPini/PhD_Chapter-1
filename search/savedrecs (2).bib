@article{ WOS:000264201500018,
Author = {Negreiros, Daniel and Fernandes, G. Wilson and Silveira, Fernando A. O.
   and Chalub, Clarissa},
Title = {Seedling growth and biomass allocation of endemic and threatened shrubs
   of rupestrian fields},
Journal = {ACTA OECOLOGICA-INTERNATIONAL JOURNAL OF ECOLOGY},
Year = {2009},
Volume = {35},
Number = {2},
Pages = {301-310},
Month = {MAR-APR},
Abstract = {The increasing anthropogenic pressure in the rare rupestrian fields in
   southeastern Brazil has led to the expansion of degraded areas on the
   extremely nutrient-deficient quartzitic soils. on the other hand, the
   use of rupestrian field native species in reclamation programmes has
   been hampered by the lack of studies involving seedling physiological
   ecology. The present study evaluated biomass allocation and seedling
   growth rate during early seedling growth of four Fabaceae shrubs:
   Collaea cipoensis, Calliandra fasciculata, Chamaecrista ramosa, and
   Mimosa foliolosa. The following hypotheses were tested: (i) species
   proportionally allocate higher biomass to the roots, presenting a high
   root/shoot ratio; and (ii) species exhibit low phenotypic variation
   because they have adapted to poor nutritional environments. A 12-month
   greenhouse experiment was carried out to evaluate seedling growth and
   biomass allocation performance in substrates with contrasting levels of
   soil fertility, The four species studied presented values of root/shoot
   ratio lower than one in both fertility conditions of the substrate.
   Growth parameters for Collaea and Calliandra increased with increasing
   soil fertility, while no differences were observed for Mimosa and
   Chamaecrista. Although the four species are naturally adapted to low
   nutritional quality soils, seedling development was not hindered by high
   fertility substrate conditions. Despite the remarkable differences in
   fertility between the substrates, the responsiveness in growth and
   allocation in Chamaecrista and Mimosa was lower than that expected if
   the species would exhibit high phenotypic variation. The implications
   for rupestrian field restoration are discussed. (C) 2008 Elsevier Masson
   SAS. All rights reserved.},
Type = {Article},
Affiliation = {Fernandes, GW (Corresponding Author), Univ Fed Minas Gerais, Ecol Evolut \& Biodiversidade DBG, ICB, Av Antonio Carlos 6627,CP 486, BR-30161970 Belo Horizonte, MG, Brazil.
   Negreiros, Daniel; Fernandes, G. Wilson; Silveira, Fernando A. O.; Chalub, Clarissa, Univ Fed Minas Gerais, Ecol Evolut \& Biodiversidade DBG, ICB, BR-30161970 Belo Horizonte, MG, Brazil.},
DOI = {10.1016/j.actao.2008.11.006},
Keywords = {Biomass partitioning; Land rehabilitation; Phenotypic variation;
   Rupestrian fields; Serra do Cipo},
Keywords-Plus = {PLANT-COMMUNITIES; ABANDONED FIELDS; SOIL FERTILITY; FOREST;
   RESTORATION; CONSERVATION; LIMITATION; STRATEGIES; PATTERNS; OUTCROPS},
Author-Email = {gwilson@icb.ufmg.br},
