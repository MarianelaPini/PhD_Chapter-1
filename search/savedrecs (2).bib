DOI = {10.20502/rbg.v22i1.1826},
Keywords = {River Styles; Channel Morphology; Rectification; Juturnaiba Lagoon},
Keywords-Plus = {CATCHMENT},
Author-Email = {rcdscorreia@gmail.com
   monicamarcal@gmail.com},
