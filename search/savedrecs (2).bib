   Silva, Arystides Resende; Lopes, Jose do Carmo Alves; Schwartz, Gustavo, Forest Ecol \& Management, Embrapa Eastern Amazonia, BR-66095903 Belem, PA, Brazil.},
DOI = {10.1007/s11258-024-01434-5},
