   Neves, F. S., Univ Fed Minas Gerais, Belo Horizonte, MG, Brazil.
   Schoereder, J. H., Univ Fed Vicosa, Vicosa, MG, Brazil.},
DOI = {10.13102/sociobiology.v64i3.1276},
Keywords = {Formicidae; Spatial scales; Temporal scales; beta-diversity;
   Conservation strategies},
Keywords-Plus = {HYMENOPTERA-FORMICIDAE; RESOURCE AVAILABILITY; INSECT HERBIVORES;
   SPECIES RICHNESS; ARBOREAL ANTS; LEAF DAMAGE; COMMUNITIES; CONSERVATION;
