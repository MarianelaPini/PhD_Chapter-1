   Sementili-Cardoso, Guilherme; Vianna, Renata Marques; Ottonicar, Rafael Gustavo Capinzaiki, Sao Paulo State Univ, Inst Biosci, Botucatu, SP, Brazil.},
DOI = {10.1080/00222933.2021.1887385},
