@article{ WOS:000537458600006,
Author = {dos Santos, Jussiara Dias and Mucida, Danielle Piuzana and Dias Gonzaga,
   Anne Priscila and de Jesus Franca, Luciano Cavalcante and Menezes,
   Eduarda Soares},
Title = {FROM THE 19TH TO THE 21ST CENTURY: A COMPARATIVE STUDY OF PRIMITIVE
   VEGETATION BY SAINT-HILAIRE AND CURRENT PHYTOPHYSIOGNOMIES},
Journal = {FINISTERRA-REVISTA PORTUGUESA DE GEOGRAFIA},
Year = {2020},
Volume = {55},
Number = {113},
Pages = {117-134},
Abstract = {The objective of this work is to compare the vegetation domains
   described by Saint-Hilaire at 1817 to current phytophysionomic
   classifications and ecotone areas for the region between Diamantina,
   Serro and Minas Novas, Minas Gerais, Brazil. A database was elaborated
   from the analysis of his works with localities, species and descriptions
   of the vegetation strata for the trajectory he traveled and steps he
   covered in the field. Current vegetation and land cover maps were
   elaborated using the ArcGIS 10.3.1 software. Three paths were defined
   from the analysis of the traveller's works. Vegetation domains such as
   virgin forest, catingas, carrasqueiros, carrascos and natural
   grasslands, described by Saint Hilaire for Minas Gerais were described
   in the study area and compared to the current phytophysiognomies:
   Semi-deciduous Seasonal Forests, Deciduous Seasonal Forests; Open
   Arboreal Caatinga; Dense, Typical, Ralo and Rupestrian Cerrado; Dirty,
   Clean and Rupestrian Grasslands respectively. The naturalist describes
   anthropic domains called capoeira and artificial grasslands whose
   vegetation is at different stages of ecological succession. Comparisons
   between old and current descriptions have proved to be feasible and
   relevant. This information contributes with ecological, biological and
   geographical data, subsidizing studies and strategies for the
   conservation of the natural resources of Biomes.},
