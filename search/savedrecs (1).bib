   Alves, Luciana F., Univ Colorado, INSTAAR, Boulder, CO 80309 USA.},
DOI = {10.1016/j.landurbplan.2010.11.003},
Keywords = {Atlantic forest; Aulonemia aristulata; Bamboo; Disturbance;
