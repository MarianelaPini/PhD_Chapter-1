@article{ WOS:000297644400006,
Author = {Volpato Sccoti, Marta Silvana and Araujo, Maristela Machado and Wendler,
   Cristiane Friedrich and Longhi, Solon Jonas},
Title = {NATURAL REGENERATION MECHANISMS IN A SEASONAL DECIDUOUS FOREST FRAGMENT},
Journal = {CIENCIA FLORESTAL},
Year = {2011},
Volume = {21},
Number = {3},
Pages = {459-472},
Month = {JUL-SEP},
Abstract = {Due to the loss of biodiversity and natural habitat because of the
   forests fragmentation, it is the needed to find alternatives that allow
   the recovery of these environments. Thus, this study aimed to obtain
   information about the potential of natural regeneration mechanisms (seed
   rain, soil seed bank, seedling bank and established natural
   regeneration) in a seasonal deciduous forest fragment in Santa Maria -
   RS state, in order to conserve and to recover these ecosystems. The
   study was conducted in a systematic way, having as a point of departure
   the demarcation of 14 sampling plots of 2000 sq m. From those plots, 70
   subplots were randomly selected to evaluate the natural regeneration
   mechanisms. The seed rain was evaluated during one year based on the
   material that was monthly collected and analyzed from the collectors. To
   the study of the soil seed bank, soil samplings of 25cmx25cmx5cm were
   taken and the collected material was monitored during 7 months,
   observing the seed germination. The natural regeneration was evaluated
   in two classes: seedling bank and established natural regeneration,
   where individuals h >= 30 cm and DBH <= 5 cm were identified and
   measured. The seed rain presented medium density of 1350 seeds m(-2), in
   which 73 species, predominantly arboreal, were observed. In the soil
   seed bank, 108 species were observed, in which 74 \% were herbaceous. In
   the seedling bank, 48 species were found and they were heliophilous and
   sciaphilous species, while in the established natural regeneration, 37
   species were found, prevailing sciaphilous. This study concluded that
   the species with the greatest potential to perpetuate in the studied
   area were Gymnanthes concolor, Soroceae bonplandii, Eugenia rostrifolia,
   Trichilia claussenii, Trichilia elegans and Dasyphylum spinescens, and
   they are highly indicated to the enrichment of the area. The species
   Myrocarpus frondosus, Cupania vernalis, Nectandra megapotamica and
   Syagrus rommanzoffiana showed certain restriction, depending on the
   silvicultural treatments in the forest to assure their perpetuation in
   the area.},
Type = {Article},
Affiliation = {Sccoti, MSV (Corresponding Author), Univ Fed Santa Maria, Programa Posgrad Engn Florestal, Ctr Ciencias Rurais, Av Roraima 1000, BR-97105900 Santa Maria, RS, Brazil.
   Volpato Sccoti, Marta Silvana, Univ Fed Santa Maria, Programa Posgrad Engn Florestal, Ctr Ciencias Rurais, BR-97105900 Santa Maria, RS, Brazil.
   Araujo, Maristela Machado; Longhi, Solon Jonas, Univ Fed Santa Maria, Dept Ciencias Florestais, Ctr Ciencias Rurais, BR-97105900 Santa Maria, RS, Brazil.
   Wendler, Cristiane Friedrich, Univ Fed Santa Maria, Acad Curso Grad Engn Florestal, Ctr Ciencias Rurais, BR-97105900 Santa Maria, RS, Brazil.},
Keywords = {seed rain; soil seed bank; seedling bank; established natural
   regeneration},
Author-Email = {martasccoti@yahoo.com.br},
Times-Cited = {18},
