   Nogueira Ferraz, Elba Maria, Inst Fed Educ Ciencia \& Tecnol Pernambuco, BR-50740540 Recife, PE, Brazil.},
Keywords = {Caatinga; density; dynamics; rainfall; seedling recruitment; semi-arid},
Keywords-Plus = {NATURAL REGENERATION; DESERT; COMMUNITY; DIVERSITY; ABUNDANCE; DYNAMICS;
   GRASS; SEED},
