DOI = {10.1111/btp.12383},
Keywords = {Amazon; Atlantic Forest; Forest Code; large-scale restoration; natural
   regeneration; restoration methods; restoration monitoring},
Keywords-Plus = {LAND-COVER CHANGE; ATLANTIC FOREST; ECOSYSTEM SERVICES; SECONDARY
   FORESTS; NATURAL FOREST; REGENERATION; RESILIENCE; DIVERSITY; AMAZON;
   DEFORESTATION},
Author-Email = {pedrob@usp.br},
