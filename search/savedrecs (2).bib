Keywords = {logging; impacts; machine traffic; soil compaction; forest regeneration},
Keywords-Plus = {SOIL PHYSICAL-PROPERTIES; ORGANIC-MATTER REMOVAL; TROPICAL RAIN-FOREST;
   CUT-TO-LENGTH; BOREAL FOREST; BULK-DENSITY; CHEMICAL-PROPERTIES; LOGGING
   EQUIPMENT; LOBLOLLY-PINE; INFILTRATION CAPACITY},
