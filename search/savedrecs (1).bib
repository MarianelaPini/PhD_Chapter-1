}

@inproceedings{ WOS:000426954603004,
Author = {Menezes, D. P. and Mantovani, W. and Fonseca Filho, H. and Conti, L. A.
   and Bittencourt, Jr., C.},
Book-Group-Author = {IEEE},
Title = {APPLYING GEOGRAPHIC OBJECT-BASED IMAGE ANALYSIS (GEOBIA) AND DATA MINING
   TO IDENTIFY SECONDARY FORESTS SUCCESSION ON SANTAREM REGION, PARA,
   BRAZIL},
