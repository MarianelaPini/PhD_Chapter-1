Keywords = {Corymbia citriodora; Ecological indicators; Edaphic fauna; Regeneration},
Keywords-Plus = {BIODIVERSITY; QUALITY; LITTER; INVERTEBRATES; COLEOPTERA; COLLEMBOLA;
