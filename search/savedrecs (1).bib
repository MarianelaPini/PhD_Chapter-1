DOI = {10.1007/s10750-012-1360-1},
Keywords = {Land use change; Rural landscapes; Forest fragmentation; Degraded
   streams},
Keywords-Plus = {LAND-USE; NEW-ZEALAND; HABITAT FRAGMENTATION; BENTHIC
