@article{ WOS:000719844300001,
Author = {Jones, Harrison H. and Barreto, Elisa and Murillo, Oscar and Robinson,
   Scott K.},
Title = {Turnover-driven loss of forest-dependent species changes avian species
   richness, functional diversity, and community composition in Andean
   forest fragments},
Journal = {GLOBAL ECOLOGY AND CONSERVATION},
Year = {2021},
Volume = {32},
Month = {DEC},
Abstract = {Andean forests, a hotspot of biodiversity, have suffered extensive
   fragmentation, yet we have little understanding of how this process has
   affected biodiversity. We surveyed bird communities across a gradient of
   fragment sizes (10-170 ha) and a continuous forest reference site in the
   Colombian Western Andes. Using a multi-species occupancy model to
   combine survey data from audio-visual transect surveys, mist netting,
   and playbacks for owls, we estimated alpha and beta taxonomic and
   functional diversity. We asked whether (1) habitat amount (patch size),
   edge effects, or selective logging affect bird occupancy and drive
   changes to diversity, (2) functional and taxonomic diversity respond
   similarly to fragmentation, and (3) compositional changes result from
   species turnover or nested species loss. Species richness declined with
   decreasing habitat amount, increasing edge density, and increasing
   disturbance through selective logging. These effects were driven by the
   loss of forest-dependent species, which were also area sensitive: 30
   such species were absent from fragments, even the largest ones (>150
   ha). Area-sensitive species were also edge sensitive and increased in
   occupancy in unlogged forest. We further found high beta diversity
   (0.78) driven by species turnover (85\% of dissimilarity) along the
   gradient. Despite extensive turnover to non-forest species within
   functional groups, functional trait richness and dispersion
   significantly declined with habitat amount. Small fragments may mimic
   the structure and composition of early-successional Andean forests,
   driving spatial turnover patterns favoring disturbance-adapted species
   at the expense of primary-forest specialists. Large forest reserves are
   therefore required to conserve forest-dependent Andean birds.},
Type = {Article},
Affiliation = {Jones, HH (Corresponding Author), Univ Florida, Dept Biol, Bartram Hall,876 Newell Dr, Gainesville, FL 32611 USA.
   Jones, Harrison H.; Robinson, Scott K., Univ Florida, Dept Biol, Bartram Hall,876 Newell Dr, Gainesville, FL 32611 USA.
   Jones, Harrison H.; Robinson, Scott K., Univ Florida, Florida Museum Nat Hist, Dickinson Hall,1659 Museum Rd, Gainesville, FL 32611 USA.
   Barreto, Elisa, Univ Fed Goias, Dept Ecol, BR-74690900 Goiania, Go, Brazil.
   Barreto, Elisa, Swiss Fed Inst Forest Snow \& Landscape, Birmensdorf, ZH, Switzerland.
   Murillo, Oscar, Univ Valle, Dept Biol, Bldg 320,Calle 13 100-00, Cali, Colombia.},
DOI = {10.1016/j.gecco.2021.e01922},
