Keywords = {Leaf area index; Leaf biomass; Root ingrowth technique; Fine-root
   production; Seasonality},
Keywords-Plus = {SLASH-AND-BURN; RAIN-FOREST; ABOVEGROUND BIOMASS; DEEP ROOTS; SHIFTING
