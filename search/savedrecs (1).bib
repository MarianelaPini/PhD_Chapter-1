@article{ WOS:000249270300014,
Author = {Guimaraes, Ima Celia and Proctor, John},
Title = {Mechanisms of plant regeneration during succession after shifting
   cultivation in eastern Amazonia},
Journal = {PLANT ECOLOGY},
Year = {2007},
Volume = {192},
Number = {2},
Pages = {303-315},
Month = {OCT},
Abstract = {The role of seed bank, seed rain, and regeneration from seedlings and
   sprouts after swidden agriculture was compared in 5-, 10- and
   20-year-old secondary forest and in a primary forest in Bragantina,
   Para, Brazil. The seed bank (0-5 cm soil depth) was largest in the
   5-year-old forest (1190 +/- 284 seeds m(-2)) and decreased nearly
   ten-fold with age to 137 +/- 19 seeds m(-2) in the primary forest. The
   highest seed rain was in the 5-year-old forest (883 +/- 230 seeds m(-2)
   year(-1)) and the least in the primary forest (220 +/- 80 seeds m(-2)
   year(-1)). Large plants (>= 5 cm dbh) had more individuals and species
   that regenerated from sprouts than from seeds and the most abundant tree
   species in the secondary forest stands of all ages appear to be
   maintained by sprouting. The smaller individuals (>= 1 m tall, < 5 cm
   dbh) in the 5-year-old forest were mainly from sprouts, but those in the
   older secondary forests originated mainly from seeds. These results show
   that at the beginning of succession, although many species can be
   introduced to swidden fallow from seed bank and seed rain, it is the
   sprout that is the main source of recruits of primary forest species in
   secondary forests in Bragantina.},
Type = {Article},
Affiliation = {Guimaraes, IC (Corresponding Author), Museu Paraense Emilio Goeldi, Dept Bot, CP 399, BR-66040170 Belem, Para, Brazil.
   Museu Paraense Emilio Goeldi, Dept Bot, BR-66040170 Belem, Para, Brazil.
   Univ Stirling, Sch Biol \& Environm Sci, Stirling FK9 4LA, Scotland.},
DOI = {10.1007/s11258-007-9327-4},
Keywords = {Amazonia; seed bank; seed rain; secondary succession; slash-and-burn;
   succession; sprouting},
Keywords-Plus = {BARRO-COLORADO-ISLAND; RIO NEGRO REGION; SOIL SEED BANK; ABANDONED
   PASTURE; BURN AGRICULTURE; BURIED SEEDS; RAIN-FOREST; GAP-PHASE;
   DYNAMICS; BASIN},
