@article{ WOS:000332194000008,
Author = {Grombone-Guaratini, Maria Tereza and Alves, Luciana Ferreira and Vinha,
   Daniella and Daher Correa Franco, Geraldo Antonio},
Title = {Seed rain in areas with and without bamboo dominance within an urban
   fragment of the Atlantic Forest},
Journal = {ACTA BOTANICA BRASILICA},
Year = {2014},
Volume = {28},
Number = {1},
Pages = {76-85},
Month = {JAN-MAR},
Abstract = {Understanding the flow of diaspores is fundamental for determining plant
   population dynamics in a particular habitat, and a lack of seeds is a
   limiting factor in forest regeneration, especially in isolated forest
   fragments. Bamboo dominance affects forest structure and dynamics by
   suppressing or delaying the recruitment of and colonization by tree
   species as well as by inhibiting the survival and growth of adult trees.
   The goal of the present study was to determine whether dominance of the
   bamboo species Aulonemia aristulata (Doll) McClure in the forest
   understory influences species abundance and composition. We examined the
   seed rain at two noncontiguous sites (1.5 km apart) within an urban
   forest fragment, with and without bamboo dominance (BD+ and BD- areas,
   respectively). Sixty seed traps (0.5 m(2), with a 1-mm mesh) were set in
   the BD+ and BD areas, and the seed rain was sampled from January to
   December 2007. Diaspores were classified according to dispersal
   syndrome, growth form and functional type of the species to which they
   belonged. There were significant differences between the two areas in
   terms of seed density, species diversity and dispersal syndrome. The BD+
   area showed greater seed density and species diversity. In both areas,
   seed distribution was limited primarily by impaired dispersal. Bamboo
   dominance and low tree density resulted in fewer propagules in the seed
   rain. Our results suggest that low availability of seeds in the rain
   does not promote the maintenance of a degraded state, characterized by
   the presence of bamboo.},
Type = {Article},
Affiliation = {Grombone-Guaratini, MT (Corresponding Author), Inst Bot Sao Paulo, Sao Paulo, Brazil.
   Grombone-Guaratini, Maria Tereza, Inst Bot Sao Paulo, Sao Paulo, Brazil.
   Alves, Luciana Ferreira, Inst Agron Campinas, Ctr Pesquisa \& Desenvolvimento Recursos Genet, Campinas, SP, Brazil.
   Vinha, Daniella, Univ Estadual Campinas, Campinas, SP, Brazil.
   Daher Correa Franco, Geraldo Antonio, Inst Florestal Sao Paulo, Div Dason, Sao Paulo, Brazil.},
DOI = {10.1590/S0102-33062014000100008},
Keywords = {Aulonemia aristulata; dispersal; human activity; natural regeneration},
Keywords-Plus = {AULONEMIA-ARISTULATA; TREE REGENERATION; RECRUITMENT; LIMITATION;
   UNDERSTORY; LIANAS; SASA; BANK},
Author-Email = {mgromboneguaratini@gmail.com},
