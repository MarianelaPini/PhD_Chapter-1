@article{ WOS:001040169100001,
Author = {Piaia, Bruna Balestrin and Rovedder, Ana Paula Moreira and Procknow,
   Djoney and Fockink, Guilherme Diego and Camargo, Betina},
Title = {Taxonomic and functional diversity of applied nucleation for ecological
   restoration},
Journal = {PLANT ECOLOGY},
Year = {2023},
Volume = {224},
Number = {10},
Pages = {855-864},
Month = {OCT},
Abstract = {Nucleation has been introduced as a promising method for ecological
   restoration and represents an efficient, as well as economical,
   alternative to existing methods of restoration to accelerate secondary
   succession in degraded sites. In this study, we evaluated ecological
   indicators of taxonomic diversity and functional diversity of planted
   trees and subsequent natural regeneration individuals to verify the
   success of restoration in three sites (Cacapava do Sul, Minas do Leao,
   and Pantano Grande) with applied nucleation carried out seven years ago
   in Rio Grande do Sul State, in south of Brazil. All shrub and tree
   regenerating or planted individuals with a height greater than 30 cm
   were counted, identified, and measured in 5 m x 5 m plots, totaling 575
   m(2) evaluated in each site. The results show that richness of
   regenerating species was similar in each site, but the richness of
   planted species was lower in Minas do Leao, intermediate in Cacapava do
   Sul, and higher in Pantano Grande. We verify a difference in the
   floristic composition of planted and regenerating species. The
   functional diversity indices were similar between planted and
   regenerating, with the exception of functional dispersion, although
   Minas do Leao presents low functional richness (0.13). This indicates
   that even with lower species richness and lower functional richness in
   planting does not imply lower species richness and lower functional
   richness of regenerants. Applied nucleation can be an efficient method
   for ecological restoration and favors the micro-habitat alteration and
   the return of propagules, even with lower initial richness of planted
   species.},
