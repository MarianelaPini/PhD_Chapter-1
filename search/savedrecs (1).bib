@article{ WOS:000355797400003,
Author = {Cysneiros, Vinicius Costa and Mendonca-Junior, Joaquim de Oliveira and
   Gaui, Tatiana Dias and Braz, Denise Monte},
Title = {Diversity, community structure and conservation status of an Atlantic
   Forest fragment in Rio de Janeiro State, Brazil},
Journal = {BIOTA NEOTROPICA},
Year = {2015},
Volume = {15},
Number = {2},
Abstract = {The State of Rio de Janeiro still holds unknown portions of the Atlantic
   Forest, which represent gaps in the knowledge of this ecosystem.
   Paracambi and neighbor municipal districts comprise a vast stretch of
   virtually unknown forest that makes up part of the Serra do Mar
   biodiversity corridor. The following study describes the tree community
   structure in a stretch of the ombrophilous Atlantic Forest, in the
   Parque Natural Municipal do Curio (Curio MNP), Paracambi, Rio de
   Janeiro, which serves as the basis to evaluate species diversity and
   conservation status. The altitudes in the area ranges from 100 to 690 m
   and the climate varies from mild subtropical to tropical hot and humid.
   Forty plots (10x10m) were randomly placed along the area, where all the
   stems equal to or greater than five centimeters (5 cm) diameter at
   breast high from ground level were measured. A total of 749 individuals,
   pertaining to 128 genera and 210 species were recorded, eight of which
   are listed as threatened of extinction in the Brazilian flora. The
   Shannon-Weaver diversity index (H') was 4.7 nat.ind.(-1) and the species
   with greater importance value was Pseudopiptadenia contorta (1.6460
   m(2); 18 individuals) while the most common species were Senefeldera
   vertcilata (48) and Actinostemon verticilatus (47 individuals). Despite
   the sample area being a little smaller than that commonly adopted in
   tropical studies, data pointed to a richness hitherto not registered in
   any other study for the State of Rio de Janeiro, no matter the inclusion
   criteria used, and also confirmed by the two estimator indices used,
   which have revealed very similar results. The vertical forest structure,
   the values associated to the successional stage of the species and other
   aspects of community structure indicate that the Curio MNP encompasses
   forest stretches in advanced stages of ecological succession. Besides
   its key location for the connectivity of forest fragments in the region,
   the results emphasize the ecological importance of this forest remnant
   and the need for its conservation.},
Type = {Article},
Affiliation = {Cysneiros, VC (Corresponding Author), Univ Fed Rural Rio de Janeiro, Inst Biol, Dept Bot, Seropedica, RJ, Brazil.
   Cysneiros, Vinicius Costa; Mendonca-Junior, Joaquim de Oliveira; Gaui, Tatiana Dias; Braz, Denise Monte, Univ Fed Rural Rio de Janeiro, Inst Biol, Dept Bot, Seropedica, RJ, Brazil.},
DOI = {10.1590/1676-06032015013214},
Keywords = {tropical rain forest; phytosociology; ecological succession; Serra do
   Mar biodiversity corridor; Curio Municipal Natural Park- Paracambi},
Keywords-Plus = {SPECIES ACCUMULATION CURVE},
Author-Email = {vccysneiros.florestal@gmail.com},
Times-Cited = {8},
Unique-ID = {WOS:000355797400003},
DA = {2025-04-03},
}

@article{ WOS:001023758300001,
Author = {de Oliveira, Viseldo Ribeiro and Ribeiro, Andressa and Pareyn, Frans
   Germain Corneel and Drumond, Marcos Antonio and Porto, Diogo Denardi and
   Kiill, Lucia Helena Piedade and Ferraz Filho, Antonio Carlos},
Title = {The influence of clear-cut area size on forest regrowth: a case study in
   the dry tropical forests of Pernambuco, Brazil},
Journal = {CERNE},
Year = {2023},
Volume = {29},
Abstract = {Background: Managed Caatinga forests in Northeast Brazil are an
   important source of wood products, however, successful regeneration and
   regrowth is important to guarantee sustainability in these forests. The
   main objective of this study was to evaluate the natural regeneration
   and forest regrowth ability of a Caatinga forest under varying clear-cut
   treatments, as well as to estimate the recovery time of stand
   parameters.Results: An experiment was set up in Petrolina, Pernambuco
   state, Brazil, comparing five clear-cut strip widths: 0, 40, 60, 80 and
   100 meters. Forest inventory data was gathered before and eight years
   after harvesting in 19 plots of 10 x 40 m. Tree seedling regeneration
   (individuals with circumference at breast height < 6 cm and minimum
   height of 0.5 m) was also monitored in 5 x 5 m sub plots, before
   harvesting, one, three and eight years after harvesting.Conclusions: No
   influence of the clear-cut strip width on regeneration and forest
   regrowth ability was detected, with all treatments presenting similar
   growth and tree species diversity and similarity values. The estimated
   growth rates of the clear-cut plots were of 0.12 m2 \& BULL;ha-1 \&
   BULL;year-1 and 0.39 m3 \& BULL;ha-1 \& BULL;year-1 for basal area and
   volume, respectively, resulting in recovery times of 35 and 49 years,
   longer than the 15 years cycles generally adopted in Caatinga forest
   management plans.},
Type = {Article},
Affiliation = {Ribeiro, A (Corresponding Author), Univ Fed Piaui, Bom Jesus, Piaui, Brazil.
   de Oliveira, Viseldo Ribeiro; Drumond, Marcos Antonio; Porto, Diogo Denardi; Kiill, Lucia Helena Piedade, Embrapa Semiarido, Petrolina, PE, Brazil.
   Ribeiro, Andressa; Ferraz Filho, Antonio Carlos, Univ Fed Piaui, Bom Jesus, Piaui, Brazil.
   Pareyn, Frans Germain Corneel, Assoc Plantas Nordeste, Recife, PE, Brazil.},
DOI = {10.1590/01047760202329013165},
Article-Number = {e103165},
Keywords = {Forest Management; Forest Regeneration; Seasonally Dry Tropical Forests},
Keywords-Plus = {CAATINGA; MANAGEMENT},
Author-Email = {andressa.florestal@ufpi.edu.br},
Times-Cited = {0},
Unique-ID = {WOS:001023758300001},
DA = {2025-04-03},
}

@article{ WOS:000333086300003,
Author = {Suganuma, M. S. and de Assis, G. B. and Durigan, G.},
Title = {Changes in plant species composition and functional traits along the
   successional trajectory of a restored patch of Atlantic Forest},
Journal = {COMMUNITY ECOLOGY},
Year = {2014},
Volume = {15},
Number = {1},
Pages = {27-36},
Month = {JUN},
Abstract = {Studies on the dynamics of ecosystems undergoing restoration are needed
   to verify whether they are following the expected trajectory, developing
   in unexpected ways, or becoming stabilized in a non desirable
   intermediate stage of blocked succession. In order to elucidate the
   successional trajectory of the plant community in a 20 ha patch of
   riparian Atlantic Forest (southeastern Brazil) undergoing restoration,
   we assessed native tree and shrub species regenerating at 18, 28 and 38
   years after planting. We analyzed changes in floristic composition and
   proportions of functional traits, by comparison with the set of 166
   species originally planted most of which were non-native, and with the
   plant assemblages of two reference riparian ecosystems - a primary-type
   and a secondary forest, in the same eco-region. Despite isolation from
   extant forests, immigrating native species have enriched and dominated
   the community undergoing restoration. Thus, the floristic composition
   and the proportions of species among functional guilds is becoming more
   distinct through time from the set of species planted and more similar
   to the nearest secondary forest (1.6 km), but is still dramatically
   different from that of a primary forest 50 km away. The proportions of
   functional guilds among individuals regenerating have shown stability
   over time but differ in general from the primary forest, particularly
   for the higher proportion of zoochorous plants in the forest undergoing
   restoration. The proportion of non-native species as well as of
   individuals of these species in the community have decreased over time,
   even though they were the majority of the species planted, refuting the
   hypothesis of priority effect driving the community assembly.},
Type = {Article},
Affiliation = {Suganuma, MS (Corresponding Author), Univ Sao Paulo, Sao Paulo, Brazil.
   Suganuma, M. S., Univ Sao Paulo, Sao Paulo, Brazil.
   de Assis, G. B., Sao Paulo State Univ UNESP, Agron Sci Fac, Dept Nat Resources, Sao Paulo, Brazil.
   Durigan, G., Forestry Inst Sao Paulo State, Assis State Forest, Brazil.},
DOI = {10.1556/COMEC.15.2014.1.3},
Keywords = {Assembly rules; Community dynamics; Natural regeneration; Reference
   ecosystem; Tropical riparian forest},
Keywords-Plus = {VERTEBRATE DISPERSAL SYNDROMES; SEED DISPERSAL; ECOLOGICAL RESTORATION;
   NATURAL REGENERATION; TROPICAL FORESTS; RAIN-FOREST; DIVERSITY;
   VEGETATION; PATTERNS; CHRONOSEQUENCE},
