@article{ WOS:000275597600015,
Author = {Cortines, Erika and Valcarcel, Ricardo},
Title = {INFLUENCE OF PIONEER-SPECIES COMBINATIONS ON RESTORATION OF DISTURBED
   ECOSYSTEMS IN THE ATLANTIC FOREST, RIO DE JANEIRO, BRAZIL},
Journal = {REVISTA ARVORE},
Year = {2009},
Volume = {33},
Number = {5},
Pages = {927-936},
Month = {SEP-OCT},
Abstract = {The analysis of species composition and its effects on sustainability
   restoration processes in the Atlantic Forest with poor environmental
   attributes is important to improve rehabilitation techniques for
   disturbed ecosystems. Reforestation projects were used as Biological
   Measures (BM) of rehabilitation, where treatments differ in the
   composition of exotic species, utilized as anthropic pioneers: BM1 -
   82\% (73\% Mimosa caesalpiniifolia Benth, 9\% Eucalyptus citriodora
   Hook.); BM2 - 91\% (9\%, 82\%); and BM3 - 25\% (15\%, 10\%). The
   monitoring of spontaneous regeneration was evaluated in three
   12-year-old reforestation sites between thr rainy season of 2004 and
   2005, and compared with an approximately 100-year-old native forest
   fragment and a grassland: ecosystems with inertial tendency toward
   recuperation and degradation, respectively. It was detected that exotic
   species used as anthropic pioneers strongly influenced regeneration: BM1
   (75\%), BM2 (85\%), BM3 (55\%), Forest (0\%) and Grassland (50\%). The
   highest similarity of species with forest regeneration (5\%) was found
   for treatment BM3.},
Type = {Article},
Affiliation = {Cortines, E (Corresponding Author), Univ Fed Rural Rio de Janeiro, Inst Florestas, Dept Ciencias Ambientais, Rio De Janeiro, Brazil.
   Cortines, Erika; Valcarcel, Ricardo, Univ Fed Rural Rio de Janeiro, Inst Florestas, Dept Ciencias Ambientais, Rio De Janeiro, Brazil.},
DOI = {10.1590/S0100-67622009000500015},
Keywords = {Disturbed areas; resilience and spontaneous regeneration},
Keywords-Plus = {ABANDONED PASTURE; TREE PLANTATIONS; ESTABLISHMENT; REGENERATION;
   SUCCESSION; DIVERSITY; RECOVERY},
Author-Email = {ecortines@gmail.com},
