@article{ WOS:001198111200001,
Author = {Goncalves, Driele S. A. and Oliveira, Patricia C. and Goncalves, Stela
   R. A. and Silveira, Fernando A. O. and Teixido, Alberto L.},
Title = {Seeding the Pantanal: orchestrating research to overcome restoration
   barriers in the world's largest tropical wetland},
Journal = {RESTORATION ECOLOGY},
Year = {2024},
Volume = {32},
Number = {5},
Month = {JUL},
Abstract = {Knowledge on seed ecology is foundational for effective seed-based
   restoration including seedling production, direct seed sowing, topsoil
   transplant, and natural regeneration. Consequently, quantifying
   available knowledge and biases in seed-based research allows
   practitioners to better plan and implement seed-based restoration
   programs and identify research priorities. Using a systematic review
   comprising 45 papers, 401 species, and 2,415 case studies, we provide an
   overview of seed ecology research in the Pantanal, the world's largest
   tropical wetland. We identified geographic, taxonomic, and ecological
   biases, as well as knowledge gaps in seed functions including
   production, dispersal, dormancy, germination, predation, and stress
   tolerance. Studies concentrated near large cities resulting in extensive
   unexplored sites in central Pantanal, in Paraguay, and in Bolivia.
   Unexpectedly, the most studied species were conducted with dry-fruited,
   terrestrial forbs, or shrubs with autochoric or zoochoric dispersal
   syndromes. Seed banks, stress tolerance, and dispersal were the most
   studied topics, whereas studies on dormancy and seedling establishment
   remain rare. We also found disproportionate research interest across the
   angiosperm tree of life with many examples of underrepresented and
   overrepresented families. Altogether, persistent knowledge gaps in
   seed-based research in the Pantanal hinder upscaling restoration
   programs and consequently the likelihood of achieving the targets of UN
   Decade of Ecosystem Restoration. We propose an integrated agenda
   consisting of a series of orchestrated actions to overcome such
   barriers, close biodiversity knowledge shortfalls, and promote
   successful large-scale restoration programs in the Pantanal.},
Type = {Review},
Affiliation = {Teixido, AL (Corresponding Author), Univ Fed Mato Grosso, Dept Bot \& Ecol, BR-78060090 Cuiaba, MT, Brazil.
   Goncalves, Driele S. A.; Oliveira, Patricia C.; Goncalves, Stela R. A.; Teixido, Alberto L., Univ Fed Mato Grosso, Dept Bot \& Ecol, BR-78060090 Cuiaba, MT, Brazil.
   Silveira, Fernando A. O., Univ Fed Minas Gerais, Dept Genet Ecol \& Evolucao, BR-31270901 Belo Horizonte, MG, Brazil.
   Teixido, Alberto L., Univ Complutense Madrid, Fac Ciencias Biol, Dept Biodivers Ecol \& Evoluc, Madrid 28040, Spain.},
DOI = {10.1111/rec.14148},
EarlyAccessDate = {APR 2024},
Keywords = {aquatic plants; ecological bias; geographic bias; seed ecology;
   taxonomic bias; wetland},
Keywords-Plus = {CONSERVATION; REGENERATION; FORESTS},
Author-Email = {alteixido@ucm.es},
Times-Cited = {2},
