@article{ WOS:000405453200007,
Author = {do Nascimento, Paulo and da Silva, Geraldo Aparecido and da Silva,
   Aderlan Gomes and Alves Barbosa, Altamiro Lauro and Sant'anna de Mello
   Lopez, Victor Hugo},
Title = {Evaluation of population density and natural regeneration of jucara palm
   (Euterpe edulis Mart.) in the countryside of Sao Joao
   Evangelista-MG},
Journal = {REVISTA AGROGEOAMBIENTAL},
Year = {2016},
Volume = {8},
Number = {2},
Pages = {73-83},
Month = {JUN},
Abstract = {This work was developed in a population of E. edulis (jucara palm), in
   an area of 4.5 hectares, located on Nova Granja farm of 363 acres, in
   Sao Joao Evangelista, MG. The study aimed to analyze the population
   structure of this species in a seasonal semideciduous montane forest,
   understand its dynamics and provide information that may be useful to
   the sustainable management of E. edulis in this type of environment. The
   population data were collected in 10 plots of 10 x 15 m, divided into
   three transects, 175 m apart from each other, arranged in a
   northeast-southwest position. Data from five developmental stages (Youth
   I, Youth II, Immature I, Immature II and Adult) were collected and the
   density and regeneration of this population were evaluated. A large
   percentage of young population ``I{''} was found. Adults whose leaf
   insertion height varied from 14.3 to 18.3 m were the most frequent class
   in that population. Population density was relatively high (0.74/m(2));
   population distribution was in inverted ``J{''} and it was found in a
   natural condition of preservation.},
