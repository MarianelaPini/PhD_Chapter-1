DOI = {10.1016/j.jag.2008.04.001},
Keywords = {Secondary succession; Multiangular; Hyperspectral; CHRIS/PROBA; Amazonia},
Keywords-Plus = {CLASSIFICATION ACCURACY; FOREST REGENERATION; EASTERN AMAZONIA; SURFACE;
   ANGLE; MISR},
