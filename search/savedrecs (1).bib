@article{ WOS:000376657700001,
Author = {Campos, Wanuza Helena and Martins, Sebastiao Venancio},
Title = {NATURAL REGENERATION STRATUM AS AN INDICATOR OF RESTORATION IN AREA OF
   ENVIRONMENTAL COMPENSATION FOR MINING LIMESTONE, MUNICIPALITY OF
   BARROSO, MG, BRAZIL},
Journal = {REVISTA ARVORE},
Year = {2016},
Volume = {40},
Number = {2},
Pages = {189-196},
Month = {MAR-APR},
Abstract = {This study was conducted in a forest under restoration process, which
   belongs to the company Holcim Brasil S/A, in the municipality of
   Barroso, state of Minas Gerais (21 degrees 00' to 22 degrees 00'S and 43
   degrees 00' to 44 degrees 00'W), where 40 plots (2 x 2 m) were set,
   spaced at 10 m, forming eight strata parallel to the watercourse present
   in the area. Floristic composition and natural regeneration stratum were
   characterized, and the formed strata allowed evaluating whether the
   riparian vegetation and watercourse influence on the local regeneration.
   It was found 162 individuals of 13 families, 18 genera and 22 species,
   and 10,125 individuals/ha were estimated. Successional classes from
   pioneer and early secondary and zoochory dispersion syndrome prevailed
   among species and individuals. The watercourse and riparian vegetation
   did not exercise significant influence (p>0.05) on the number of species
   and regenerating individuals among the different strata of the forest.
   The diversity index of Shannon-Wiener (H') and equability of Pielou (J')
   were 2.691 and 0.870, respectively. The species Psidium guajava and
   Myrtaceae families presented the highest VI (value of importance).
   Natural regeneration analysis showed the low floristic diversity in the
   area, suggesting that corrective management actions should be adopted.},
Type = {Article},
Affiliation = {Campos, WH (Corresponding Author), Univ Fed Vicosa, Dept Engn Florestal, Programa Posgrad Ciencia Florestal, Vicosa, MG, Brazil.
   Campos, Wanuza Helena, Univ Fed Vicosa, Dept Engn Florestal, Programa Posgrad Ciencia Florestal, Vicosa, MG, Brazil.
   Martins, Sebastiao Venancio, Univ Fed Vicosa, Dept Engn Florestal, Ctr Ciencias Agr, Vicosa, MG, Brazil.},
DOI = {10.1590/0100-67622016000200001},
Keywords = {Ecological restoration; Forest restoration; Restoration indicators},
Keywords-Plus = {SEASONAL SEMIDECIDUOUS FOREST; ECOLOGICAL RESTORATION; DISPERSAL; VICOSA},
Author-Email = {wanuzacampos@yahoo.com.br
   venancio@ufv.br},
Times-Cited = {7},
Unique-ID = {WOS:000376657700001},
DA = {2025-04-03},
}

@article{ WOS:000419994800106,
Author = {Macaneiro, J. P. and Gasper, A. L. and Schorn, L. A. and Galvao, F.},
Title = {FEW DOMINANT NATIVE WOODY SPECIES: HOW SUBTROPICAL RAINFOREST
   SUCCESSIONAL PROCESS ACTS ON ABANDONED PASTURES IN SOUTHERN BRAZIL},
Journal = {APPLIED ECOLOGY AND ENVIRONMENTAL RESEARCH},
Year = {2017},
Volume = {15},
Number = {4},
Pages = {1633-1676},
Abstract = {In Brazil, a high number of woody species have been used for forest
   restoration plantings. However, this action is widely questionable due
   to the no re-establishment of a considered ``normal{''} successional
   trajectory. In this study, we analyzed subtropical rainforest natural
   regeneration at abandoned pastures and we showed that vegetation is
   characterized, predominantly, by few high dominant native woody species,
   highlighting especially Vernonanthura discolor (Asteraceae), Myrsine
   coriacea (Primulaceae), and Piptocarpha regnellii (Asteraceae). Our
   results also indicate that these high dominance species favor the lowest
   diversity of natural regeneration. In this regard, we suggest
   implementing actions that provide improvements and facilitate natural
   processes of ecological succession by planting dominant native woody
   species. This takes into consideration the re-establishment of a
   considered ``normal{''} successional trajectory.},
Type = {Article},
Affiliation = {Maçaneiro, JP (Corresponding Author), Univ Fed Parana Box, Dept Engn Florestal, BR-80210170 Curitiba, Parana, Brazil.
   Macaneiro, J. P.; Galvao, F., Univ Fed Parana Box, Dept Engn Florestal, BR-80210170 Curitiba, Parana, Brazil.
   Gasper, A. L., Fundacao Univ Reg Blumenau Box, Dept Ciencias Nat, BR-89030103 Blumenau, Brazil.
   Schorn, L. A., Fundacao Univ Reg Blumenau Box, Dept Engn Florestal, BR-89030000 Blumenau, Brazil.},
DOI = {10.15666/aeer/1504\_16331676},
Keywords = {forest regeneration; ``normal{''} successional trajectory; dominant
   species; species diversity; CSR ecological strategies},
Keywords-Plus = {SANTA-CATARINA STATE; ATLANTIC FOREST; NATURAL REGENERATION;
   RESTORATION; PLANT; CONSERVATION; DIVERSITY; MECHANISMS; MICROSITES;
   VEGETATION},
Author-Email = {jpmacaneiro@gmail.com},
Times-Cited = {5},
