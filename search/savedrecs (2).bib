@article{ WOS:A1995QD64600004,
Author = {SALOMAO, RD and ROSA, NA and NAPSTAD, DC and BAKK, A},
Title = {DIAMETRICAL STRUCTURE AND BRIEF ECOLOGICAL ECONOMICAL CHARACTERIZATION
   OF THE 108 TREE SPECIES OF THE BRAZILIAN AMAZON FOREST},
Journal = {INTERCIENCIA},
Year = {1995},
Volume = {20},
Number = {1},
Pages = {20-29},
Month = {JAN-FEB},
Abstract = {Twenty-one botanical stocks, of one hectare each, were inventoried, in
   the States of Para (17), Maranhao (3) and Rondonia (1).  In total,
   10,257 individuals were registered, with diameter at breast height (DBH)
   > or < 10 cm. belonging to 790 species, distributed among 68 families. 
   One hundred and eight species (14\% of the total) presented 20 or more
   individuals, totalizing 7,011 (68\%) individuals; these species are
   analyzed in the present paper in relation to:  1) growth in diameter and
   thickness of the trunk, 2) vertical stratification, 3) natural
   regeneration and 4) usefulness of the species.
   The individuals of each of the 108 species were grouped in 5 classes
   according to growth of the trunk, namely:  a) Small (DBH < 30 cm.) 45
   (42\%) species; b) Medium, between 30 and 70 cm. 43 (40\%) species; c) 5
   (5\%) species; d) very big DBH (between 100 and 200 cm.) 5 (4\%) species
   and e) Giant (DBH > 200 cm.) 2 (2\%) species.
   The vertical stratification, obeys the following criteria:  a) emerging
   (total height greater than 40 m.) 6 (6\%) species; b) Bunting (height
   between 25 and 40 m.), 54 (50\%) species and, c) sub forest (height less
   than 10m) 2 (2\%) species.
   Natural regeneration, analyzed with respect to the availability of
   light, allowed us to classify the 108 species studied in three distinct
   groups:  a) Full sunthey regenerate in open environments - total 43
   (40\%) species; b) wood clearing - need direct light for part of the
   day, 100 (93\%) species and shadow - regenerate only in the absence of
   direct light, 8 (7\%) species.
   The present usefulness of the species was approached under two large
   groups:  value of the wood and non-wood use(s).  The species with wood
   value were grouped under the following categories:  a) Very high value
   (m3 sawed wood:  greater than U.S. \$400.00) no species were registered
   in this category; b) High value, 16 species registered (15\%); c) medium
   value, 16 (15\%) species were registered and d) low value (m3 of sawed
   wood less than U.S. \$80.00) 38 (35\%) species.  Of the 108 species
   studied, 38 (35\%) had at present no value in the internal and external
   markets.  The non-wood uses are:  a) food for man and fauna, (10
   species); b) food exclusively for the fauna, (85 species); c) fiber
   providers, (13 species); d) medical application, (12 species); e)
   production of gum, latex or essential oil (8.8 and 9 species)
   respectively; f) cellulose industry, (5 species); g) ornamental use, (4
   species); h) trunk bark with anti-corrosive properties, (1 species);
   aromatic, (1 species); and silicose, (1 species); application with
   tincture (2 species); and venomous substance for cattle, (1 species).
   Finally, various comments are made on the most characteristic species of
   the humid dense Amazonian Forest.},
Type = {Article},
Affiliation = {SALOMAO, RD (Corresponding Author), MUSEU PARAENSE EMILIO GOELDI,DEPT BOT,CP 399,BR-66017970 BELEM,PARA,BRAZIL.
   WOODS HOLE RES CTR,WOODS HOLE,MA 02543.},
Times-Cited = {5},
Unique-ID = {WOS:A1995QD64600004},
DA = {2025-04-03},
}

@article{ WOS:000339929900005,
Author = {Beckert, Sara Moreira and Doetzer Rosot, Maria Augusta and Rosot, Nelson
   Carlos},
Title = {Growth and dynamics of Araucaria angustifolia (Bert.) O. Ktze. in
   an Ombrophylous Mixed Forest remnant},
Journal = {SCIENTIA FORESTALIS},
Year = {2014},
Volume = {42},
Number = {102},
Pages = {209-218},
Month = {JUN},
Abstract = {In order to understand the dynamics and growth processes of Araucaria
   angustifolia (Bert.) O. Ktze., this study evaluated the behavior of the
   following variables: number of trees, diameter, basal area. ha(-1) and
   volume.ha(-1) of this species over eight consecutive years by the
   measurement of permanent plots established in a remnant of Araucaria
   Forest, located in Cacador - SC, which presents large individuals of A.
   angustifolia and an understory occupied by bamboos. Therefore, two
   hypotheses were evaluated: i) growth and yield of the variables tend to
   remain stable or decrease over time and ii) recruitment of new
   individuals is low or nonexistent. The results show a reduction in the
   number of trees, in average basal area and stand volume per hectare due
   to mortality and absence of natural regeneration. There was an average
   increase about 3 cm in diameter, ranging from 75.95 cm to 78.57 cm, and
   the mean periodical annual increment (PAI) of 0.33 cm.year(-1). The
   average basal area decreased by 4.80 m(2).ha(-1), ranging from 32.75
   m(2).ha(-1) to 27.95 m(2).ha(-1). Average volume per hectare decreased
   by 78.38 m(3).ha(-1) with a negative PAI of 9.8 m(3).ha(-1).year(-1).
   Statistically significant differences were noted mainly in response to
   weather events that have raised the rate of mortality in the third and
   sixth years of measurement.},
Type = {Article},
Affiliation = {Beckert, SM (Corresponding Author), UFPR Univ Fed Parana, Av Pref Lothario Meissner,900 Jardim Bot Campus 3, BR-80210170 Curitiba, Parana, Brazil.
