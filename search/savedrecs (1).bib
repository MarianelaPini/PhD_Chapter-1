@article{ WOS:000249280200009,
Author = {Schoengart, Jochen and Wittmann, Florian and Worbes, Martin and
   Fernandez Piedade, Maria Teresa and Krambeck, Hans-Juergen and Junk,
   Wolfgang Johannes},
Title = {Management criteria for Ficus insipida Willd.: (Moraceae) in
   Amazonian white-water floodplain forests defined by tree-ring analysis},
Journal = {ANNALS OF FOREST SCIENCE},
Year = {2007},
Volume = {64},
Number = {6},
Pages = {657-664},
Month = {SEP},
Abstract = {Ficus insipida Willd. (Moraceae) is a fast growing tree species of early
   successional stages in the Amazonian nutrient-rich white-water
   floodplains (varzea). The species is one of the most economically
   important low-density wood species in the community-based forest
   management project in the Mamiraua Sustainable Development Reserve
   (MSDR) in Central Amazonia, where timber species are managed using a
   polycyclic selection system with a minimum logging diameter (MLD) of 50
   cm and a cutting cycle of 25 years. In this study we analyze the
   floristic composition, stand structure and forest regeneration of a
   natural 20 year-old stand at an early successional stage and we model
   tree growth of diameter, height and volume of F. inspida based on
   tree-ring analysis to define management criteria. The volume growth
   model indicates that the preferred period for logging should be at a
   tree age of 17 years when the current annual volume increment peaks.
   This age corresponds to a diameter of 55 cm, which would be an
   appropriate MLD.},
Type = {Article},
Affiliation = {Schöngart, J (Corresponding Author), Max Planck Inst Limnol, August Thienemannstr 2,POB 165, D-24302 Plon, Germany.
   Max Planck Inst Limnol, D-24302 Plon, Germany.
   Univ Gottingen, Inst Agron Trop, D-37077 Gottingen, Germany.
   Inst Nacl de Pesquisas da Amazonia, BR-69011970 Manaus, Amazonas, Brazil.},
DOI = {10.1051/forest:2007044},
Keywords = {amazon; floodplain forest; tree ring; cutting cycle; minimum logging
   diameter (MLD)},
