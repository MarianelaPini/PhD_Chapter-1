DA = {2025-04-03},
}

@article{ WOS:000738622000017,
Author = {Duarte, Edilaine and Higuchi, Pedro and da Silva, Ana Carolina and
   Guerra Sobral, Marcos Eduardo and da Costa Bortoluzzi, Roseli Lopes and
   de Almeida, Jaime Antonio and Larsen, Janaina Gabriela and Dallabrida,
   Juliana Pizutti and Muzeka, Luran Monteiro and de Aguiar, Jessica
   Thalheimer and Cuchi, Tarik},
Title = {Key factors affecting succession in upper montane forest areas of
   ``Planalto Sul Catarinense{''} Region, Brazil},
Journal = {BOSQUE},
Year = {2021},
Volume = {42},
Number = {3},
Pages = {353-364},
Abstract = {The understanding of the succession process is a fundamental condition
   in defining strategies for the conservation and sustainable use of
   forest ecosystems. Therefore, we aimed at testing which ecological
   factors allect short-term successional changes in the upper-montane
   forests of the ``Planalto Sul Catarinense{''} Region, Brazil. We
   evaluated the influence of propagules source, soil seed bank, edaphic
   and topographic variables, nucleating elements and natural regeneration
   floristic-structural composition on demographic rates of tree-shrub
   regenerating component in three disturbed open vegetation areas at Sao
   Joaquim National Park. We inventoried the regenerative component in
   2014, 2015 and 2016, within one 20x100 m transect in each area.
   Afterwards, demographic rates were determined for each period. The
   influence of explicative variables on demographic rates of the
   regenerating component was tested through the Generalized Least Square
   (GLS) model. We found an increment in both individuals and richness over
   time. When analyzing the influence of the explanatory variables on the
   speed of the successional process, we observed that only the
   floristic-structural composition of the natural regeneration was
   determinant. We conclude that the speed of the represented succession
   is, to a large extent, a feature related to the phase of the
   successional trajectory in which the vegetation is found.},
Type = {Article},
Affiliation = {Higuchi, P (Corresponding Author), Univ Estado Santa Catarina, Dept Engn Florestal, Av Luiz de Camoes 2090, Lages, SC, Brazil.
