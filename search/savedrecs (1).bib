   Penuela, M. C., Univ Reg Amazan IKIAM, Salvador, BA, Brazil.},
DOI = {10.1016/j.aquabot.2015.02.001},
Keywords = {Mortality; Recruitment; Survival; Swamp forest; Black water},
Keywords-Plus = {DISTRIBUTION PATTERNS; VARZEA FORESTS; PALM; VEGETATION; COMMUNITY;
   DYNAMICS; CLASSIFICATION; HETEROGENEITY; DIVERSITY; GRADIENTS},
