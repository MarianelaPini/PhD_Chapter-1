@article{ WOS:000206047900001,
Author = {James, Preston E.},
Title = {TRENDS IN BRAZILIAN AGRICULTURAL DEVELOPMENT},
Journal = {GEOGRAPHICAL REVIEW},
Year = {1953},
Volume = {43},
Number = {2},
Pages = {301-328},
Month = {JUL},
Abstract = {The traditional agricultural system of Brazil is based on speculative
   gain and invested in monoculture. This is marked by 4 periods: (1) sugar
   period of 16th and 17th centuries; (2) gold and diamond mining of 18th
   century; (3) coffee planting of the 19th century terminating in 1930;
   (4) speculation in rubber, cacao, oranges, sisal, etc. The basic support
   of rural settlement is pasturing of cattle. The terrain is hilly to
   mountainous. Current trends indicate that in spite of an increasingly
   rapid migration from rural districts into expanding cities, the rural
   population has continued to grow. Crop production has risen but the
   productivity of the individual rural farm worker is low, and declining
   because of the persistent use of manual implements. Agriculture is thus
   wasteful, inefficient and costly. When production drops the land is
   abandoned to pasturage. With the destruction of tropical forest closest
   to urban markets by exploitative practices, there is a continued rise of
   food prices due to transportation costs. The traditional system of land
   rotation is destroying a large part of the virgin areas, and the end of
   uncut tropical forest is in sight. Scientific farming has been
   demonstrated in Brazil, but since there are no speculative profits,
   little capital is invested. Application of the principles of scientific
   agriculture might change the pattern of cultivation. Although scientific
   farming is feasible and will increase productivity and lower the cost of
   food production, it will decrease population in thinly centered areas
   and make for a further concentration in the already densely populated
   places.},
Type = {Article},
DOI = {10.2307/211750},
Times-Cited = {5},
Unique-ID = {WOS:000206047900001},
DA = {2025-04-03},
}

@article{ WOS:000295232500003,
Author = {Amaral Nascimento, Alexandre T. and Schmidlin, Lucia A. J. and
   Valladares-Padua, Claudio B. and Matushima, Eliana R. and Verdade,
   Luciano M.},
Title = {A Comparison of the Home Range Sizes of Mainland and Island Populations
   of Black-Faced Lion Tamarins (Leontopithecus caissara) Using
   Different Spatial Analysis},
Journal = {AMERICAN JOURNAL OF PRIMATOLOGY},
Year = {2011},
Volume = {73},
Number = {11},
Pages = {1114-1126},
Month = {NOV},
Abstract = {The critically endangered black-faced lion tamarin, Leontopithecus
   caissara, has a restricted geographical distribution consisting of small
   mainland and island populations, each with distinct habitats in coastal
   southeastern Brazil. Necessary conservation management actions require
   an assessment of whether differences in habitats are reflected in use of
   space by the species. We studied two tamarin groups on the mainland at
   Sao Paulo state between August 2005 and March 2007, and compared the
   results with data from Superagui Island. Three home range estimators
   were used: minimum convex polygon (MCP), Kernel, and the new technique
   presented dissolved monthly polygons (DMP). These resulted,
   respectively, in home ranges of 345, 297, and 282 ha for the 12-month
   duration of the study. Spatial overlap of mainland groups was extensive,
   whereas temporal overlap was not, a pattern that indicates resource
   partitioning is an important strategy to avoid intraspecific
   competition. L. caissara large home ranges seem to be dynamic, with
   constant incorporation of new areas and abandonment of others through
   time. The main difference between mainland and island groups is the
   amount and variety of sleeping sites. A better understanding of the home
   range sizes, day range lengths, and territorial behavior of this species
   will aid in developing better management strategies for its protection.
   Additionally, the presented DMP protocol is a useful improvement over
   the MCP method as it results in more realistic home range sizes for
   wildlife species. Am. J. Primatol. 73: 1114-1126, 2011. (C) 2011 Wiley
   Periodicals, Inc.},
