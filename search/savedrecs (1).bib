Affiliation = {Texas A\&M Univ, Dept Entomol, Knowledge Engn Lab, College Stn, TX 77843 USA.
   Texas A\&M Univ, Dept Entomol, Knowledge Engn Lab, College Stn, TX 77843 USA.
   Texas A\&M Univ, Dept Forest Sci, Knowledge Engn Lab, College Stn, TX 77843 USA.
   Escola Super Agraria, Dept Florestal, Inst Politecn Braganca, BR-5301854 Braganca Paulista, SP, Brazil.
   Oklahoma State Univ, Dept Zool, Stillwater, OK 74078 USA.
   USDA ARS, Beneficial Insects Res Unit, Honey Bee Grp, Weslaco, TX 78596 USA.},
DOI = {10.1016/j.foreco.2005.05.005},
Keywords = {feral honey bees; forest landscape structure; mitotype diversity; Apis
