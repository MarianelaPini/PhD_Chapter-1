   NATL COUNCIL RUBBER TAPPERS, BR-69900 RIO BRANCO, ACRE, BRAZIL.},
DOI = {10.1017/S0376892900031428},
Keywords-Plus = {TROPICAL FORESTS; AMAZONIA; DEFORESTATION; ATMOSPHERE; SUCCESSION;
   LANDSCAPE},
Times-Cited = {30},
Unique-ID = {WOS:A1992LA36500005},
