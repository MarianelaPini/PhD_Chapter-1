   Indiana State Univ, Dept Geog Geol \& Anthropol, Terre Haute, IN 47809 USA.},
DOI = {10.1080/01431160412331269733},
Keywords-Plus = {ENDMEMBER SELECTION; MIXING MODELS; TM DATA; VEGETATION; ACCURACY;
   ABUNDANCE; FORESTS; IMAGES},
Author-Email = {dlu@indiana.edu},
Times-Cited = {55},
