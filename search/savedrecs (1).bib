DOI = {10.1590/S0100-67622008000600014},
Keywords = {Seed bank; forest litter and environmental recovery},
Author-Email = {tourinhobraga@yahoo.com.br
   griffith@ufv.br
   j.meira@ufv.br},
Times-Cited = {29},
