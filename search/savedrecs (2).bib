@article{ WOS:000813968900003,
Author = {Eric, Andre Nicolini and Beauchene, Jacques and Bonnal, Vincent and
   Hattermann, Tom},
Title = {Chlordecone in basal trunk wood of native trees growing in abandoned
   banana plantations in Guadeloupe, France},
Journal = {BOIS ET FORETS DES TROPIQUES},
Year = {2022},
Number = {352},
Pages = {31-42},
Abstract = {Chlordecone (CLD), used to control the Cosmopolites sordidus banana
   weevil and released from polluted banana plantations, continues to
   contaminate ecosystems in the French Caribbean. Edible plants have been
   actively studied for risk prevention, but trees have not, even though
   they could play a significant role in future remediation strategies. CLD
   contents were analysed in 24 pioneer trees belonging to 13 species found
   in abandoned banana plantations in Guadeloupe, at three contaminated
   sites on Nitisols (Site 1) and Andosols (Sites 2 and 3). Samples were
   taken from each tree: wood in the basal part of the trunk and soil at
   its foot and their CLD content was measured by the analytical laboratory
   for the Dr??me d??partement (26) in Valence, France. Mean CLD contents
   in the top 30 -centimetre soil layer from sites 1, 2 and 3 were 2,543 ??
   702, 5,251 ?? 1,102 and 875 ?? 865 pg/kg dry soil respectively. Of the
   trees, 96\% were contaminated. The CLD content in trees growing on
   Nitisols (3,406 ?? 1,658 pg/kg dry wood) was at least 5 times higher
   than in trees growing on Andosols (299 ?? 314 and 226 ?? 378 pg/kg), but
   no clear relationships were found with soil CLD contents. Calculations
   of available CLD dissolved in the soil liquid phase using equations and
   soil datasets in the literature showed higher available CLD contents in
   Nitisols than in Andosols and a linear relationship between CLD
   available in soil and concentrations of CLD in wood, regardless of the
   type of soil. Trees growing on Nitisols are the plants most highly
   contaminated by CLD of all the plants in which this compound has been
   studied so far. With a plant-to-soil bioconcentration ratio around 150
   l/kg, the consistent CLD uptake efficiency of the trees needs to be
   taken into account in further research for CLD remediation.},
Type = {Article},
Affiliation = {Eric, AN (Corresponding Author), Univ Montpellier, AMAP, CIRAD, CNRS,INRAE,IRD, Montpellier, France.
   Eric, Andre Nicolini; Hattermann, Tom, Univ Montpellier, AMAP, CIRAD, CNRS,INRAE,IRD, Montpellier, France.
   Beauchene, Jacques, Univ Guyane, Univ Antilles, CNRS,INRA, CIRAD,UMR Ecol Forets Guyane EcoFoG,AgroParisTech, F-97310 Kourou, France.
   Bonnal, Vincent, Univ Brasilia, Ctr Desenvolvimento Sustentave, UMR TETIS, CIRAD, Campus Univ Darcy Ribeiro, BR-70910900 Brasilia, DF, Brazil.},
DOI = {10.19182/bft2022.352.a36937},
Keywords = {Wood; Chlordecone; forest trees; native; Guadeloupe; France},
Keywords-Plus = {ORGANIC-MATTER; SOIL; CONTAMINATION; MODEL; AREA},
Author-Email = {eric-andre.nicolini@cirad.fr},
Times-Cited = {1},
Unique-ID = {WOS:000813968900003},
DA = {2025-04-03},
}

@incollection{ WOS:000289443400004,
Author = {Schroeder, Wilfrid and Alencar, Ane and Arima, Eugenio and Setzer,
   Alberto},
