@article{ WOS:000369916700037,
Author = {Poorter, Lourens and Bongers, Frans and Aide, T. Mitchell and Almeyda
   Zambrano, Angelica M. and Balvanera, Patricia and Becknell, Justin M.
   and Boukili, Vanessa and Brancalion, Pedro H. S. and Broadbent, Eben N.
