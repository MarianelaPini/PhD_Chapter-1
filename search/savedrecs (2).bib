@article{ WOS:000982885100001,
Author = {Rueger, Nadja and Schorn, Markus E. and Kambach, Stephan and Chazdon,
   Robin L. and Farrior, Caroline E. and Meave, Jorge A. and Munoz, Rodrigo
   and van Breugel, Michiel and Amissah, Lucy and Bongers, Frans and
