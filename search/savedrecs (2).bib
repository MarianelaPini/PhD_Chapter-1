   Vieira, Daniel L. M., Embrapa Recursos Genet \& Biotecnol, Lab Ecol \& Conservacao, BR-70770900 Brasilia, DF, Brazil.},
DOI = {10.1111/rec.12252},
Keywords = {bud bank; cerrado; mining; Neotropical savanna; reclamation; resprouting
   ability},
Keywords-Plus = {RESPROUTING ABILITY; SEED BANK; FOREST},
Author-Email = {daniel.vieira@embrapa.br},
Times-Cited = {55},
