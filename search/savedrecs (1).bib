@article{ WOS:000292308000010,
Author = {Silva, C. E. M. and Goncalves, J. F. C. and Alves, E. G.},
Title = {Photosynthetic traits and water use of tree species growing on abandoned
   pasture in different periods of precipitation in Amazonia},
Journal = {PHOTOSYNTHETICA},
Year = {2011},
Volume = {49},
Number = {2},
Pages = {246-252},
Month = {JUN},
Abstract = {Pasture soils in the Amazon become unsustainable after a short period of
   use, typically being replaced by emergent secondary vegetation
   (capoeira). The aim of this research was to investigate the
   photosynthetic capacity and water use in the most common tree species
   (Vismia japurensis, Vismia cayennensis, Bellucia grossularioides, Laetia
   procera, and Goupia glabra) in successional chronosequence. This study
   was carried out in secondary vegetation area with ages that vary between
   1 and 19 years. Responses of gas exchange were determined during
   different periods of precipitation. The gas exchange decreased with
   advancing age of the vegetation (1-19 years), except for G. glabra.
   Negative relationships of P (Nmax) as a function of aging observed for
   V. japurensis, V. cayennensis, B. grossularioides, and L. procera
   exhibited r (2) equal to 0.59, 0.42, 0.33, and 0.58, respectively. The
   species of Vismia showed higher values for photosynthetic parameters in
   relation to other species across the chronosequence. Overall, there were
   differences in gas exchange only for some species between the different
   periods of precipitation. Therefore, our results suggest a distinct
   pattern of photosynthetic responses to species in early succession.
   Light decrease can exert a decisive role to reduce the photosynthetic
   rates in secondary succession species. On the other hand, the results of
   WUE showed weak evidence of changes for the species during dry and rainy
   periods in the abandoned pasture in central Amazonia.},
Type = {Article},
Affiliation = {Gonçalves, JFC (Corresponding Author), Natl Inst Amazon Res INPA, POB 478, BR-69011970 Manaus, Amazonas, Brazil.
   Silva, C. E. M.; Goncalves, J. F. C.; Alves, E. G., Natl Inst Amazon Res INPA, BR-69011970 Manaus, Amazonas, Brazil.},
DOI = {10.1007/s11099-011-0033-z},
Keywords = {light-response curve; photosynthesis, secondary succession; stomatal
   conductance},
Keywords-Plus = {USE EFFICIENCY; LEAF-AREA; CARBON; NITROGEN; PIONEER; FOREST; LIGHT;
   ACCLIMATION; SUCCESSION; PLANTS},
Author-Email = {jfc@inpa.gov.br},
Times-Cited = {10},
Unique-ID = {WOS:000292308000010},
