@article{ WOS:000595160400005,
Author = {da Silva, Nelio Domingos and Marangon, Luiz Carlos and Patriota
   Feliciano Marangon, Ana Licia and Aleixo da Silva, Jose Antonio},
Title = {Dimension of plots for inventory of regenerating tree vegetation in a
   fragment of Atlantic Forest in Pernambuco, Brazil},
Journal = {SCIENTIA FORESTALIS},
Year = {2020},
Volume = {48},
Number = {127},
Month = {SEP},
Abstract = {This study aims to assess the methods of relative efficiency of the
   sampling in the definition of the area and shape of plots in surveys of
   regenerating tree in a fragment of the Atlantic Forest in Pernambuco,
   Brazil. 40 plots were installed for each of the different dimensions
   analyzed. All individuals with height >= 1.0 meter and circumference at
   breast height (1.30m above of ground) < 15.0cm were cataloged,
   identified and measured. The sampling and floristic sufficiency,
   rarefaction curves, Jackknife estimates and the relative efficiency were
   calculated and plotted for the plots dimensions of 5m x 10m, 10m x 10m
   and 10m x 25m because they satisfied the sampling error criterion
   admissible. The sampling of regenerating tree vegetation indicated that
   the plot of 10m x 10m is the most efficient given the high sampling
   precision, high species richness and low sampling time.},
Type = {Article},
Affiliation = {da Silva, ND (Corresponding Author), Univ Fed Rural Pernambuco UFRPE, Dept Ciencia Florestal, Recife, PE, Brazil.
   da Silva, Nelio Domingos; Marangon, Luiz Carlos; Patriota Feliciano Marangon, Ana Licia; Aleixo da Silva, Jose Antonio, Univ Fed Rural Pernambuco UFRPE, Dept Ciencia Florestal, Recife, PE, Brazil.},
