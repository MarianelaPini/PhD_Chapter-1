@article{ WOS:000716233800001,
Author = {Medeiros, Gabriela Garcia and Antonio, Juliana and Harrison, Michelle
   and Della Coletta, Luciana and Soltangheisi, Amin and Banks-Leite,
   Cristina and Duarte-Neto, Paulo Jose and Martinelli, Luiz Antonio},
Title = {Effect of vertebrate exclusion on leaf litter decomposition in the
   coastal Atlantic forest of southeast Brazil},
Journal = {TROPICAL ECOLOGY},
Year = {2022},
Volume = {63},
Number = {1},
Pages = {151-154},
Month = {MAR},
Abstract = {Forest fragmentation is the main cause of biodiversity loss in the
   Anthropocene and it is triggering a top-down cascade effect that may
   influence leaf litter decomposition. However, the link between
   biodiversity loss and changes in ecosystem processes is not fully
   understood. We conducted an experiment to evaluate whether the exclusion
   of vertebrates affects the decomposition rates of different litter types
   (grass, a pioneer species and a mix of litters from a well preserved
   secondary forest). To answer this question, we placed litter bags inside
   artificial exclusion plots that were built in two types of vegetation
   coverage (abandoned pastureland and preserved forest) in the Atlantic
   forest of southeast Brazil. Our hypothesis is that vertebrate exclusion
   will change decomposition rates of different litter types due to the
   top-down cascade effect on soil invertebrates. We found out that
   vertebrate exclusion did not affect decomposition rates, possibly due to
   the overlap of ecological functions in the soil fauna responsible for
   the decomposition process. Furthermore, grass litter was decomposed
   faster (k value), possibly due to its initial nutrient content and hence
   litter quality which plays a significant role on decomposition rates.},
Type = {Article},
Affiliation = {Medeiros, GG (Corresponding Author), Univ Sao Paulo, Ctr Nucl Energy Agr, BR-13400970 Piracicaba, SP, Brazil.
   Medeiros, Gabriela Garcia; Antonio, Juliana; Della Coletta, Luciana; Soltangheisi, Amin; Martinelli, Luiz Antonio, Univ Sao Paulo, Ctr Nucl Energy Agr, BR-13400970 Piracicaba, SP, Brazil.
   Harrison, Michelle; Banks-Leite, Cristina, Imperial Coll London, Dept Life Sci, Silwood Pk Campus,Buckhurst Rd, Ascot SL5 7PY, Berks, England.
   Duarte-Neto, Paulo Jose, Univ Fed Rural Pernambuco, Dept Stat \& Informat, BR-52171900 Recife, PE, Brazil.},
DOI = {10.1007/s42965-021-00189-8},
EarlyAccessDate = {NOV 2021},
Keywords = {Biodiversity loss; Decomposition; Nutrient cycling; Top-down control;
   Tropical forest; Vertebrate exclusion},
Keywords-Plus = {BIODIVERSITY; DIVERSITY; FAUNA},
Author-Email = {gabigarcia.medeiros@hotmail.com},
Times-Cited = {1},
