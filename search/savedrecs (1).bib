DA = {2025-04-03},
}

@article{ WOS:000494748000001,
Author = {David, H. C. and Fernandes, A. P. D. and Barbosa, E. S. and Neto, R. P.
   M.},
Title = {NATURAL VEGETATION DYNAMICS AROUND THE IRATI NATIONAL FOREST, BRAZIL, IN
   RESPONSE TO ENVIRONMENTAL POLICIES AND AGRICULTURAL EXPANSION FROM 1992
   TO 2017},
Journal = {JOURNAL OF TROPICAL FOREST SCIENCE},
Year = {2019},
Volume = {31},
Number = {4},
Pages = {377-383},
Abstract = {The aim of this research was to relate natural vegetation dynamics to
   environmental policies and agricultural pressure surrounding the Irati
   National Forest (INF), in the state of Parana, Brazil. We mapped
   landuses and detected their changes over a period of 25 years (1992 to
   2017), emphasising gains and losses of natural vegetation to other uses.
   Employing a pixel-based approach, Landsat-5 TM and Landsat-8 OLI data
   were used to classify landuse surrounding the INF. We reviewed records
   of agricultural expansion and environmental policies in Parana to
   explain losses and gains in vegetation cover detected in the landuse
   change analysis. During the study period the area surrounding the INF
   gained -10,000 ha of natural vegetation cover regenerated from areas
   formerly under crop or pasture cover and planted forests, but lost
   -20,000 ha, largely to agricultural expansion, likely for soybeans. We
   discuss environmental policies, particularly the Ecological ICMS (a
   Brazilian ecological fiscal incentive paid to counties that shelter
   protected areas) as the most important reason for the vegetation
   regeneration and nature conservation.},
Type = {Article},
Affiliation = {David, HC (Corresponding Author), Rural Fed Univ Amazonia, POB 68650-000, Capitao Poco, Brazil.
   David, H. C.; Fernandes, A. P. D.; Barbosa, E. S., Rural Fed Univ Amazonia, POB 68650-000, Capitao Poco, Brazil.
   Neto, R. P. M., State Univ Sao Paulo, Dept Cartog, POB 19060-900, Presidente Prudente, Brazil.},
DOI = {10.26525/jtfs2019.31.4.377},
Keywords = {Landuse change; deforestation; agricultural expansion; natural
   regeneration; Ecological ICMS},
Keywords-Plus = {STATE; FIRE},
Author-Email = {hassancamil@gmail.com},
