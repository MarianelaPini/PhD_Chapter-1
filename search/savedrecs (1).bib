   Fernandes, Luiz Arnaldo, Univ Fed Minas Gerais, Montes Claros, MG, Brazil.},
DOI = {10.1590/01047760201622012086},
Keywords = {Plant diversity; Phytosociology; Tropical wet savannah; Degradation},
Author-Email = {yule.nunes@unimontes.br},
