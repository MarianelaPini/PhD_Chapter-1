DOI = {10.15517/rbt.v69i2.41800},
Keywords = {disturbance; ecological indicators; regeneration strategy; species
   richness; dispersal syndrome},
Keywords-Plus = {TROPICAL FOREST; EXTRAPOLATION; RESTORATION; RAREFACTION; DISPERSAL;
   DYNAMICS; MODELS; AREA},
