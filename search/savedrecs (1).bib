DOI = {10.4322/natcon.00801010},
Keywords = {Ecological Succession; Natural Regeneration; Tropical Forest},
Keywords-Plus = {REGENERATION; RESTORATION; DISTURBANCE; VEGETATION; SUCCESSION;
