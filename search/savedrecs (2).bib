   Rosalino, Luis M.; Verdade, Luciano M., Univ Sao Paulo, Lab Ecol Isotopica, CENA, BR-13400970 Piracicaba, SP, Brazil.},
DOI = {10.1016/j.foreco.2012.07.002},
Keywords = {Rodents; Marsupials; Eucalyptus plantation; Conservation},
