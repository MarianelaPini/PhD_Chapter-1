DOI = {10.1016/j.foreco.2012.02.028},
Keywords = {Silviculture; Forest ecology; Functional groups; Tapajos},
Keywords-Plus = {NATURAL REGENERATION; POPULATION-STRUCTURE; CARAPA-GUIANENSIS; TROPICAL
   FOREST; GAPS; MANAGEMENT; DIVERSITY; RECOVERY; CANOPY; GROWTH},
Author-Email = {Gustavo.Schwartz@wur.nl},
