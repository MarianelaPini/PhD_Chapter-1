   Guilherme, E., Univ Fed Acre, Ctr Ciencias Biol \& Nat, Lab Ornitol, Rodovia BR-364,Km 04, BR-69920900 Rio Branco, Acre, Brazil.},
DOI = {10.1007/s40415-020-00662-z},
EarlyAccessDate = {OCT 2020},
