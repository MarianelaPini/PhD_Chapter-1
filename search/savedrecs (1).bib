@article{ WOS:000254729500014,
Author = {Bihn, Jochen H. and Verhaagh, Manfred and Braendle, Martin and Brandl,
   Roland},
Title = {Do secondary forests act as refuges for old growth forest animals?:
   Recovery of ant diversity in the Atlantic forest of Brazil},
Journal = {BIOLOGICAL CONSERVATION},
Year = {2008},
Volume = {141},
Number = {3},
Pages = {733-743},
Month = {MAR},
Abstract = {The extent to which secondary forests occupying degraded and abandoned
   lands provide suitable habitat for forest-adapted species is an
   important conservation issue in times of vanishing old growth forests.
   We used ants (Hymenoptera: Formicidae), a functionally important and
   diverse group of invertebrates, to investigate the recovery of soil taxa
   during secondary forest succession in the Atlantic Forest of Southern
   Brazil. We compared the resilience of epigeic vs. hypogeic ant
   assemblages. For this purpose we established 27 sites that encompassed a
   chronosequence from pastures to old growth forests on two contrasting
   soil types. Our results are based on a collection of 35508 individuals
   in 40 genera.
   Richness and composition of ant assemblages in secondary forests have
   recovered slowly and have not approached conditions typical of old
   growth forests. The distribution of genera along the successional stages
   was arranged in a nested pattern where ant genera of younger
   successional stages were a subset of genera present in older stages.
   Edaphic conditions had no influence on the recovery process. Overall,
   richness of ants was lower at study sites with water-logged soils than
   at sites where soils did not exhibit hydromorphic properties. The
   hypogeic ant assemblage recovered more slowly than the epigeic
   assemblage.
   Our results show that secondary forests do not act as refuges for many
   forest-adapted animals which are currently restricted to discontinuous
   patches of old growth forest in the highly endangered Atlantic Forest of
   Brazil. Moreover, estimated recovery times of 50 to several hundred
   years suggest it would take much longer than previously presumed for
   complete recolonization. (c) Elsevier Ltd. All rights reserved.},
Type = {Article},
Affiliation = {Bihn, JH (Corresponding Author), Univ Marburg, Dept Anim Ecol, Karl Von Frisch Str 8, D-35032 Marburg, Germany.
   Bihn, Jochen H.; Braendle, Martin; Brandl, Roland, Univ Marburg, Dept Anim Ecol, D-35032 Marburg, Germany.
   Verhaagh, Manfred, State Museum Nat Hist Karlsruhe, Dept Entomol, D-761330 Karlsruhe, Germany.},
DOI = {10.1016/j.biocon.2007.12.028},
Keywords = {faunal recovery; Formicidae; nested subsets; soil fauna; resilience},
Keywords-Plus = {HIGHER-TAXON RICHNESS; SPECIES RICHNESS; BIOLOGICAL DIVERSITY;
