@article{ WOS:000806748900005,
Author = {Silva, Joao Henrique Constantino Sales and Barbosa, Alex da Silva and
   Gomes, Daniel da Silva and de Araujo, Moises Bittar},
Title = {Basal soil respiration in land use and cover systems in a caatinga
   enclaves moist forest},
Journal = {SEMINA-CIENCIAS AGRARIAS},
Year = {2022},
Volume = {43},
Number = {4},
Pages = {1849-1858},
Month = {JUL-AUG},
Abstract = {Deforestation and the disorderly exploitation of natural resources for
   agricultural expansion occurred at an accelerated pace in the Brejo
   microregion of the state of Paraiba, Brazil, which resulted in
   abandonment and erosion of the soil in a large part of the region. In
   view of this scenario, the present study was undertaken to investigate
   the soil respiration in systems involving different forms of land use
   and cover in caatinga enclaves moist forests (Brejo de Altitude areas).
   The study was conducted in four land use and cover systems, namely,
   pasture, mandala agriculture, agroforestry system, and forest. Microbial
   activity was estimated by quantifying the carbon dioxide (CO2) released
   in the soil respiration process, from the soil surface, and captured by
   KOH solution. Soil temperature (degrees C) and water content (\%) were
   monitored simultaneously with the analysis of soil respiration. The
   forms of land use and cover have a direct effect on the metabolic
   activity of soil organisms, and climatic factors such as soil
   temperature and moisture influence the dynamics of organic matter
   decomposition and, consequently, the release of CO2. The production of
   CO2 is higher at night than during the day, regardless of the analyzed
   systems. Among the evaluated areas, the forest showed the lowest CO2
   emissions, so it was considered a CO2 receptor in contrast to the
   pasture area, which functioned as a CO2 emitter. Management techniques
   that reduce surface temperature and increase organic matter should be
   prioritized to promote soil biota.},
Type = {Article},
Affiliation = {Silva, JHCS (Corresponding Author), Univ Fed Paraiba, UFPB, Agroecol, Agr Sci, Bananeiras, Paraiba, Brazil.
   Silva, Joao Henrique Constantino Sales; Gomes, Daniel da Silva; de Araujo, Moises Bittar, Univ Fed Paraiba, UFPB, Agroecol, Agr Sci, Bananeiras, Paraiba, Brazil.
   Barbosa, Alex da Silva, UFPB, Agr Dept, Bananeiras, Paraiba, Brazil.},
DOI = {10.5433/1679-0359.2022v43n4p1849},
Keywords = {Bioindicator; Carbon; Climate changes; Management; Soil organisms;
