DOI = {10.1590/S1676-06032011000100005},
Keywords = {forest dynamics; riparian forest; increment; mortality; recruitment},
Keywords-Plus = {NATURAL REGENERATION; MORTALITY PATTERNS; BRAZIL; RECRUITMENT;
   VEGETATION; DIVERSITY; STRICTO; GROWTH; RATES; SENSU},
