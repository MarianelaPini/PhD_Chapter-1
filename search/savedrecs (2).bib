   Velasquez, Elena; Lavelle, Patrick, IRD, UMR BIOSOL 137, F-93143 Bondy, France.},
DOI = {10.1016/j.agee.2007.07.007},
Keywords = {amazonia; agricultural successions; ecological groups; pastures;
