   dos Santos, Rubens Manoel; Sales Pereira, Diego Gualberto, Univ Fed Lavras, Programa Posgrad Bot Aplicada, Lavras, MG, Brazil.},
DOI = {10.31413/nativa.v6i5.5426},
Keywords = {biodiversity; forest dynamics; forest conservation},
Keywords-Plus = {TREE COMMUNITY DYNAMICS; MANTIQUEIRA RANGE; FLORISTIC COMPOSITION;
   MONTANE FOREST; SE BRAZIL; CONSERVATION; PATTERNS; DIVERSITY; MORTALITY;
   FRAGMENT},
