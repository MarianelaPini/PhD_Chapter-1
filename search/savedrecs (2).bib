@article{ WOS:000903646700001,
Author = {de Vasconcelos, Tatiana Cabral and Londe, Vinicius and Velasquez, Elena
   and Lavelle, Patrick and Rodrigues, Ricardo Ribeiro},
Title = {Atlantic Forest restoration recovers physical and biological properties
   in the short term, but not the general soil quality},
Journal = {LAND DEGRADATION \& DEVELOPMENT},
Year = {2023},
Volume = {34},
Number = {6},
Pages = {1842-1858},
Month = {APR 15},
Abstract = {Ecological restoration has the potential to improve and recover soil
   quality, helping to mitigate soil degradation worldwide. However, soil
   monitoring is often overlooked in restoration, preventing a
   comprehensive understanding of how restoration interventions affect soil
   quality and the time required for recovering different soil properties.
   Herein, we tested if MAR areas (moderately assisted recovery-tree
   plantings) in the early restoration stage have better soil quality (and
   ecosystem services) than UNR areas (unassisted natural
   recovery-abandoned pastures) and pastures in continuous use (PCU). We
   collected 72 soil samples in three 5-year-old MAR areas and three
   5-year-old UNR areas, one PCU, and one reference ecosystem (RE) in the
   Brazilian Atlantic Forest (Mata Atlantica). A total of 45 variables
   representing macrofauna, fertility, structure, and aggregation were
   measured and then submitted to multivariate analyses to compute
   sub-indicators and a general indicator of soil quality (GISQ). The
   richness of orders and classes, cationic exchange capacity, total
   porosity, and biogenic aggregates contributed the most to the
   sub-indicators. MAR and UNR areas had similar values to the RE for
   macrofauna, structure, and aggregation sub-indicators, but they were far
   below the RE for fertility. In addition, all areas had lower GISQ values
   than the RE. Soils were classified as high quality in RE and medium
   quality in other areas. Our study evidences that some soils' physical
   and biological properties can be rapidly recovered through forest
   restoration, but 5 years is not enough time to change the general soil
   quality and make restoration areas have better conditions than the
   previous land use. If the restoration goal is to have high soil quality
   in the short term, corrective actions should be taken to improve soil
   fertility and help the area along the desired trajectory.},
Type = {Article},
Affiliation = {Londe, V (Corresponding Author), Univ Florida, Ctr Aquat \& Invas Plants, Gainesville, FL 32611 USA.
   de Vasconcelos, Tatiana Cabral, Univ Sao Paulo, Coll Agr Luiz de Queiroz, Forest Sci Dept, Grad Program Forest Resources, Piracicaba, Brazil.
   de Vasconcelos, Tatiana Cabral; Rodrigues, Ricardo Ribeiro, Univ Sao Paulo, Coll Agr Luiz de Queiroz, Biol Sci Dept, Lab Ecol \& Forest Restorat LERF, Piracicaba, Brazil.
   Londe, Vinicius, Univ Florida, Ctr Aquat \& Invas Plants, Gainesville, FL 32611 USA.
   Velasquez, Elena, Univ Nacl Colombia, Palmira, Valle Do Cauca, Colombia.
   Lavelle, Patrick, Univ Paris Sorbonne Nouvelle, Inst Ecol \& Environm Studies IEES, Paris, France.},
DOI = {10.1002/ldr.4573},
EarlyAccessDate = {DEC 2022},
Keywords = {ecological restoration; GISQ; macroinvertebrates; soil chemistry; soil
   physics; soil monitoring},
Keywords-Plus = {VEGETATION RESTORATION; ECOSYSTEM SERVICES; AGGREGATION; INDICATOR;
   VALUATION; SUCCESS},
