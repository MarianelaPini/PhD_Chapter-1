   Suding, Katharine N., Univ Colorado, Dept Ecol \& Evolutionary Biol, Boulder, CO 80309 USA.},
DOI = {10.1007/s10531-016-1186-7},
Keywords = {Tropical forest; Biodiversity; Succession; Assembly; Regeneration},
