   Piller, Werner E., Karl Franzens Univ Graz, Inst Earth Sci, NAWI Graz Geocetr, Graz, Austria.},
DOI = {10.1016/j.palaeo.2020.109652},
Article-Number = {109652},
Keywords = {Palynology; Ostracods; Detrital zircon ages; Biostratigraphy; Neogene;
   Amazon rainforest},
