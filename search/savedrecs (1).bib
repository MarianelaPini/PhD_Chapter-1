   Dodonov, Pavel, Fed Univ Bahia UFBA, Inst Biol, Rua Barao de Jeremoabo 668, Salvador, BA, Brazil.},
DOI = {10.1007/s11258-024-01415-8},
EarlyAccessDate = {APR 2024},
Keywords = {Ecosystem functioning; Edge influence; Fire effect; Forest influence;
   Nutrient cycling; Tropical forests},
Keywords-Plus = {LEAF-LITTER; TROPICAL FOREST; ATLANTIC FOREST; NUTRIENT CONCENTRATIONS;
