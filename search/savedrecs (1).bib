   Simon, Augusto A., Empresa Grp TANAC SA, Tecn TANAGRO, BR-95780000 Montenegro, RS, Brazil.},
DOI = {10.5902/19805098458},
Keywords = {invasive plant; regeneration; structure; floristic composition},
Keywords-Plus = {STABILITY},
Author-Email = {silasmochiutti@cpafap.embrapa.br
   higa@ufpr.br
   aasimon@tanac.com.br},
