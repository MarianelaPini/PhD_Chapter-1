@article{ WOS:000087356700004,
Author = {Adams, C},
Title = {The Rocas and the Atlantic Rainforests traditional agricultural
   management by Caicaras: A review},
Journal = {INTERCIENCIA},
Year = {2000},
Volume = {25},
Number = {3},
Pages = {143+},
Month = {MAY-JUN},
Abstract = {A review of the agricultural system of the Atlantic Rainforest
   traditional Caicara populations was undertaken, through a comparative
   and historical approach of authors discussing slash-and-burn agriculture
   in Tropical Forests. A comparative numerical analysis of authors who
   have published data on Caicara gardens (rocas) was carried through,
   considering several variable involved in the system, including the
   historical-regional setting. The results obtained were: average size of
   gardens (0.42 ha), average time of cultivation (3.1 years), average
   fallow time (7.8 years), human occupation area, population size and
   cultivated area. The area needed for the continuity of the system was
   calculated for two different scenarios. The calculations presented were
   subjected to a degree of approximation and should be taken as
   indicative, as a sketch of the profile of some variables involved in
   Caicara gardens, and as a contribution for future research concerning
   this subject. The results obtained were compared with international
   data, and one of the hypotheses risen for the abandonment of agriculture
   in some Caicara populations, in the past, was the decrease in
   productivity, due to high population densities.},
Type = {Article},
Affiliation = {Adams, C (Corresponding Author), Univ Sao Paulo, Inst Biociencias, Lab Estudos Evolut Humanos, Rua Matao,Travessa 14,277, BR-05508900 Sao Paulo, Brazil.
