DOI = {10.1007/s00468-015-1232-1},
Keywords = {Assembly rules; Environmental gradient; Functional integration;
   Functional strategies; Leaf economics spectrum},
Keywords-Plus = {FUNCTIONAL TRAITS; RAIN-FOREST; DEMOGRAPHIC RATES; STEM TRAITS; LEAF;
   DIVERSITY; ECOLOGY; COORDINATION; PHOSPHORUS; STRATEGIES},
