   Fernandes Filho, Elpidio Inacio, Univ Fed Vicosa, Dept Solos, Vicosa, MG, Brazil.},
DOI = {10.1590/S0100-67622009000300019},
Keywords = {Remote sensing; classification of orbital images and Quickbird},
Author-Email = {macamaral@yahoo.com.br
   alsouza@ufv.br
   elpidio@ufv.br
   jacinto.lana@cenibra.com.br},
Times-Cited = {15},
