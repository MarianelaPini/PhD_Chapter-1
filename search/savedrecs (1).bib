DOI = {10.1590/0100-67622015000300011},
Keywords = {Taxonomy; Forest species; Ecology},
Keywords-Plus = {GERMINATION},
Author-Email = {roberioaf@yahoo.com.br
   stefanebarretto@bol.com.br},
