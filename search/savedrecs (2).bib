   Cunha-Machado, Antonio Saulo, Ctr Biotecnol Amazonia CBA, Lab Biol Mol LBM, Manaus, AM, Brazil.},
DOI = {10.1007/s10722-024-01890-y},
EarlyAccessDate = {FEB 2024},
