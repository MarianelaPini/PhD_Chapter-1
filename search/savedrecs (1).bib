   Neill, Christopher, Woods Hole Res Ctr, POB 296, Woods Hole, MA 02543 USA.
   Nagy, R. Chelsea, Univ Colorado, Earth Lab, Boulder, CO 80309 USA.
   Nagy, R. Chelsea, Univ Colorado, Dept Geog, Boulder, CO 80309 USA.},
DOI = {10.1002/eap.1478},
Keywords = {Amazon; biomass; disturbance; land use; nutrients; tropical secondary
   forest},
